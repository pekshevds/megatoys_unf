Процедура РезультатПроверкиКлючейИАдресаФоновоеЗадание(Параметры, АдресХранилища) Экспорт

	Результат = Новый Структура("КодСостояния,СообщениеОбОшибке");
	
	Настройки = РегистрыСведений.НастройкиСервисаКабинетСотрудника.НастройкиСервиса();
	
	ПараметрыОбмена = НовоеОписаниеПараметрыОбмена();
	ПараметрыОбмена.СтруктураАдреса = ОбщегоНазначенияКлиентСервер.СтруктураURI(Параметры.АдресПриложения);
	ПараметрыОбмена.Соединение 		= НовоеHTTPСоединение(ПараметрыОбмена.СтруктураАдреса);
	ПараметрыОбмена.ВерсияФормата 	= Настройки.ВерсияФормата;
	ПараметрыОбмена.ВерсияAPI 		= Настройки.ВерсияAPI;
	
	Ключи = Новый Структура("ИдентификаторКлиента,СекретКлиента", Параметры.Ключи.ИдентификаторКлиента, Параметры.Ключи.СекретКлиента);
	Ответ = РезультатПроверкиКлючейИАдреса(ПараметрыОбмена, Ключи);
	Если ЗначениеЗаполнено(Ответ.СообщениеОбОшибке) Тогда
		Результат.СообщениеОбОшибке = Ответ.СообщениеОбОшибке;
	КонецЕсли;
	
	Если Ответ.ОтветНаЗапрос = Неопределено Тогда
		Результат.КодСостояния = Неопределено;
	Иначе
		Результат.КодСостояния = Ответ.ОтветНаЗапрос.КодСостояния;
	КонецЕсли;
		
	ПоместитьВоВременноеХранилище(Результат, АдресХранилища);

КонецПроцедуры

Функция РезультатПроверкиКлючейИАдреса(ПараметрыОбмена, Ключи)
	
	Результат = Новый Структура("ОтветНаЗапрос,СообщениеОбОшибке");
	ИспользоватьВерсии = ЗначениеЗаполнено(ПараметрыОбмена.ВерсияФормата);
	
	Заголовки = Новый Соответствие;
	Заголовки.Вставить("User-Agent", КабинетСотрудникаПовтИсп.ОписаниеКлиентскогоПриложения());
	Если ИспользоватьВерсии Тогда
		Заголовки.Вставить("formatVersion", ПараметрыОбмена.ВерсияФормата);
	КонецЕсли;
	
	ДанныеТокена = РезультатПолученияНовогоТокена(ПараметрыОбмена, Ключи);
	Если ЗначениеЗаполнено(ДанныеТокена.СообщениеОбОшибке) Тогда
		Результат.СообщениеОбОшибке = ДанныеТокена.СообщениеОбОшибке;
		Возврат Результат;
	КонецЕсли;
	Токен = ДанныеТокена.Токен;
	
	Ответ = Неопределено;
	
	Заголовки.Вставить("Authorization", СтрШаблон("Bearer %1", Токен));
	Заголовки.Вставить("Content-Type", "application/json");
	РесурсСервиса = РесурсПинг();
	АдресРесурса = СтрШаблон("/%1%2%3",ПараметрыОбмена.СтруктураАдреса.ПутьНаСервере,РесурсAPI(ПараметрыОбмена.ВерсияAPI),РесурсСервиса);
	ИмяМетода = "GET";
	Запрос = Новый HTTPЗапрос(АдресРесурса, Заголовки);
	Соединение = ПараметрыОбмена.Соединение;
	Попытка
		Ответ = Соединение.ВызватьHTTPМетод(ИмяМетода, Запрос);
	Исключение
		ЗаписатьОшибкуВызова(ПараметрыОбмена, ИмяМетода, Запрос, , ОписаниеОшибки());
		Результат.СообщениеОбОшибке = НСтр("ru = 'Не удалось получить ответ сервера. Подробнее см в Журнале регистрации.'");
	КонецПопытки;
	
	Если Ответ <> Неопределено И Ответ.КодСостояния = 401 Тогда
		ПараметрыОбмена.Соединение = НовоеHTTPСоединение(ПараметрыОбмена.СтруктураАдреса);
		ДанныеТокена = РезультатПолученияНовогоТокена(ПараметрыОбмена, Ключи);
		Если ЗначениеЗаполнено(ДанныеТокена.СообщениеОбОшибке) Тогда
			Результат.СообщениеОбОшибке = ДанныеТокена.СообщениеОбОшибке;
			Возврат Результат;
		КонецЕсли;
		Токен = ДанныеТокена.Токен;
		Запрос.Заголовки.Вставить("Authorization", СтрШаблон("Bearer %1", Токен));
		Соединение = ПараметрыОбмена.Соединение;
		Попытка
			Ответ = Соединение.ВызватьHTTPМетод(ИмяМетода, Запрос);
		Исключение
			ЗаписатьОшибкуВызова(ПараметрыОбмена, ИмяМетода, Запрос, , ОписаниеОшибки());
			Результат.СообщениеОбОшибке = НСтр("ru = 'Не удалось получить ответ сервера. Подробнее см в Журнале регистрации.'");
		КонецПопытки;
	КонецЕсли;
	
	Если Ответ <> Неопределено И Ответ.КодСостояния >= 300 Тогда
		ЗаписатьОшибкуВызова(ПараметрыОбмена, ИмяМетода, Запрос, Ответ);
	КонецЕсли;
	
	Результат.ОтветНаЗапрос = Ответ;
	
	Возврат Результат;
	
КонецФункции

Функция РезультатПолученияНовогоТокена(ПараметрыОбмена, Ключи)

	Результат = Новый Структура("Токен,СообщениеОбОшибке");
	Ответ = НовыйТокен(ПараметрыОбмена, Ключи);
	Если Ответ.НеактуальныеКлючи Тогда
		Результат.СообщениеОбОшибке = НСтр("ru = 'Неактуальные ключи аутентификации.'");
	ИначеЕсли Не ЗначениеЗаполнено(Ответ.Токен) Тогда
		Результат.СообщениеОбОшибке = НСтр("ru = 'Не удалось получить токен аутентификации.'");
	Иначе
		Результат.Токен = Ответ.Токен;
	КонецЕсли;

	Возврат Результат;
	
КонецФункции

