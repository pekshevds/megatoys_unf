Процедура ПроверкаКС_1_02(КонтекстПроверки)
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("Раздел"				, НСтр("ru='Сведения о суммах налогов'"));
	Заголовки.Вставить("НомерКС"			, НСтр("ru='1.2'"));
	Заголовки.Вставить("Описание"			, 
		НСтр("ru='Если КБК в стр.3 = '") + КонтекстПроверки.ДанныеКС.КБК.УСН_Доходы + НСтр("ru=', то стр.4 <= 251 400 000'"));
	Заголовки.Вставить("ШаблонДетализации"	, НСтр("ru='%1 <= 251 400 000'"));
	Заголовки.Вставить("Нарушение"			, НСтр("ru='Ошибочная сумма налога'"));
	Заголовки.Вставить("Рекомендации"		, "Уточните сумму налога в стр.4");
	Заголовки.Вставить("Основание"			, НСтр("ru='Письмо ФНС России от 21.02.2023 № ЕА-4-15/2048@'"));
	Заголовки.Вставить("ШаблонДопСведений"	, НСтр("ru='Страница %1'"));
	ПодготовитьНастройки(КонтекстПроверки, Заголовки);
	
	Если КонтекстПроверки.Документирование Тогда
		Возврат;
	КонецЕсли;
	
	КонтекстПроверки.НастройкиКС.Вставить("КБК"				, КонтекстПроверки.ДанныеКС.КБК.УСН_Доходы);
	КонтекстПроверки.НастройкиКС.Вставить("ПредельнаяСумма"	, 251400000);
	
	ПроверкаКС_1_02_код(КонтекстПроверки);
	
