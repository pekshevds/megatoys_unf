	КонтекстПроверки.ДанныеКС.Вставить("Р0", ОбщегоНазначения.СкопироватьРекурсивно(
		ИсходнаяСтруктура.ДанныеУведомления.Титульная));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ДатаПодписи2", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=MMdd; ДП=0000"));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ДатаПодписи3", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=dd.MM; ДП=00.00"));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ГодПодписи", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=yyyy; ДП=(...)"));
	
	Р1 = Новый Массив;
	Для каждого Элемент Из ИсходнаяСтруктура.ДанныеМногостраничныхРазделов.СумИсчНалог Цикл
