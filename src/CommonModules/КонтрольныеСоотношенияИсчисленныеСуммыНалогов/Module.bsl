		СтраницаРаздела.Вставить("НомерСтраницы", 
			"Стр. " + XMLСтрока((ИсходнаяСтруктура.ДанныеМногостраничныхРазделов.СумИсчНалог.Индекс(Элемент) + 1)));
		СформироватьКодПериода(СтраницаРаздела);
		Р1.Добавить(СтраницаРаздела);
	КонецЦикла;
	
	КонтекстПроверки.ДанныеКС.Вставить("Р1", Р1);
КонецПроцедуры

Процедура СформироватьКодПериода(СтраницаРаздела)
	ПериодРаздела = ?(ЗначениеЗаполнено(СтраницаРаздела.Период), СтраницаРаздела.Период, "(...)");
	НомерМесяца = ?(ЗначениеЗаполнено(СтраницаРаздела.НомерМесКварт), СтраницаРаздела.НомерМесКварт, "(...)");
	СтраницаРаздела.Вставить("КодПериода", ПериодРаздела + "/" + НомерМесяца);
КонецПроцедуры

Процедура ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатель, Источник, НомерСтроки = Неопределено)
	Если Показатель.Заполнять Тогда
		ЗаполнитьПоказатель(КонтекстПроверки, Показатель, Источник, НомерСтроки);
		
		Если Показатель.Расшифровывать Тогда
			РасшифроватьПоказатель(КонтекстПроверки, Показатель);
		КонецЕсли;
	КонецЕсли;
КонецПроцедуры

Процедура ЗаполнитьПоказатель(КонтекстПроверки, Показатель, Источник, НомерСтроки = Неопределено)
	Перем ЗначениеВИсточнике;
	
	Если ТипЗнч(НомерСтроки) = Тип("Массив") Тогда
		НомерСтроки = СтрСоединить(НомерСтроки, "_");
	КонецЕсли; 
	
	Если ТипЗнч(Источник) = Тип("Структура") Тогда
		Если ЗначениеЗаполнено(НомерСтроки) Тогда
			Показатель.ИмяПоказателя = Лев(Показатель.ИмяПоказателя, 14) + НомерСтроки;
		КонецЕсли;
		
		ЗначениеВИсточнике = Источник[Показатель.ИмяПоказателя];
		
		Если ЗначениеВИсточнике = Неопределено Тогда
			ЗначениеВИсточнике = КонтекстПроверки.ДанныеКС.ЗначенияПоУмолчанию.Получить(Показатель.ИмяПоказателя);
		КонецЕсли; 
		
		Показатель.НомерСтраницы = ?(Источник.Свойство("НомерСтраницы"), Источник.НомерСтраницы, "");
		ЗаполнитьПоказательЗначениями(Показатель, ЗначениеВИсточнике);
	ИначеЕсли ТипЗнч(Источник) = Тип("ВыборкаИзРезультатаЗапроса") Тогда
		ЗначениеВИсточнике = Источник[Показатель.Синоним];
		
		Показатель.Отчет			= КонтекстПроверки.ДанныеКС.Отчет;
		Показатель.НомерСтраницы 	= Источник.НомерСтраницы;
			
		ЗаполнитьПоказательЗначениями(Показатель, ЗначениеВИсточнике);
		
		Если ЗначениеЗаполнено(НомерСтроки) Тогда
			Показатель.ИмяПоказателя = Лев(Показатель.ИмяПоказателя, 14) + НомерСтроки;
		КонецЕсли;
	КонецЕсли;
КонецПроцедуры

Процедура РасшифроватьПоказатель(КонтекстПроверки, Показатель)
		
	НоваяСтрока							= КонтекстПроверки.ТаблицаРасшифровки.Добавить();
	НоваяСтрока.НомерЭлемента			= КонтекстПроверки.ДанныеКС.Счетчик;
	
	НоваяСтрока.СсылкаНаОтчет			= ?(ЗначениеЗаполнено(Показатель.Отчет), 
		Показатель.Отчет, КонтекстПроверки.ДанныеКС.Отчет);
	НоваяСтрока.ИмяРаздела				= Показатель.ИмяРаздела;
	НоваяСтрока.ИмяПоказателя			= Показатель.ИмяПоказателя;
	НоваяСтрока.НомерСтраницы			= Показатель.НомерСтраницы;
	
	НоваяСтрока.ПредставлениеПоказателя = ?(ЗначениеЗаполнено(Показатель.Дополнение), 
		Показатель.Представление + ", (" + Показатель.Дополнение + ")", Показатель.Представление);
	
	Если ТипЗнч(Показатель.Значение) = Тип("Число") Тогда
		НоваяСтрока.ЗначениеПоказателя		= Показатель.Значение;
	Иначе
		НоваяСтрока.ЗначениеПоказателя		= Показатель.ЗначениеСтр;
	КонецЕсли;
	
	Показатель.Дополнение = "";
	
КонецПроцедуры

Процедура ЗаполнитьПоказательЗначениями(Показатель, ЗначениеВИсточнике)
	Если ТипЗнч(ЗначениеВИсточнике) = Тип("Число") Тогда
		Показатель.Значение = ЗначениеВИсточнике;
		Показатель.Итог = Показатель.Итог + ЗначениеВИсточнике;
	ИначеЕсли ТипЗнч(ЗначениеВИсточнике) = Тип("Строка") Тогда
		Показатель.Значение = СокрЛП(ЗначениеВИсточнике);
		Показатель.ЗначениеСтр = ?(ЗначениеЗаполнено(ЗначениеВИсточнике), Показатель.Значение, "(...)");
	ИначеЕсли ТипЗнч(ЗначениеВИсточнике) = Тип("Дата") Тогда
		Показатель.Значение = ЗначениеВИсточнике;
		Показатель.ЗначениеСтр = Формат(ЗначениеВИсточнике, "ДФ=dd.MM.yy; ДП=(...)");
	КонецЕсли;
КонецПроцедуры

Процедура ПодготовитьНастройки(КонтекстПроверки, Заголовки)
		
	КонтекстПроверки.НастройкиКС = Новый Структура;
	
	КонтекстПроверки.НастройкиКС.Вставить("Раздел"				, "");
	КонтекстПроверки.НастройкиКС.Вставить("НомерКС"				, "");
	
	КонтекстПроверки.НастройкиКС.Вставить("Описание"				, "");
	КонтекстПроверки.НастройкиКС.Вставить("Детализация"			, "");
	КонтекстПроверки.НастройкиКС.Вставить("Нарушение"			, "");
	КонтекстПроверки.НастройкиКС.Вставить("Основание"			, "");
	КонтекстПроверки.НастройкиКС.Вставить("Рекомендации"			, "");
	
	КонтекстПроверки.НастройкиКС.Вставить("ДопСведения"			, "");
	КонтекстПроверки.НастройкиКС.Вставить("Комментарий"			, "");
	
	КонтекстПроверки.НастройкиКС.Вставить("ЭтоОшибка"			, Ложь);
	
	КонтекстПроверки.НастройкиКС.Вставить("Условие"				, "");
	КонтекстПроверки.НастройкиКС.Вставить("ДляДокументации"		, "");
	
	КонтекстПроверки.НастройкиКС.Вставить("ШаблонДетализации"	, "");
	КонтекстПроверки.НастройкиКС.Вставить("ШаблонДопСведений"	, "");
	
	Для каждого ЭлементКоллекции Из Заголовки Цикл
		Если КонтекстПроверки.НастройкиКС.Свойство(ЭлементКоллекции.Ключ) Тогда
			КонтекстПроверки.НастройкиКС.Вставить(ЭлементКоллекции.Ключ, ЭлементКоллекции.Значение);
		КонецЕсли; 
	КонецЦикла;
	
	Приоритет = КонтекстПроверки.ДанныеКС.ПриоритетыРазделов.Найти(КонтекстПроверки.НастройкиКС.Раздел);
	Приоритет = ?(Приоритет = Неопределено, 0, Приоритет + 1);
	
	КонтекстПроверки.НастройкиКС.Вставить("Приоритет"			, Приоритет);
	
	Если КонтекстПроверки.Документирование Тогда
		КонтекстПроверки.ДанныеКС.Счетчик = КонтекстПроверки.ДанныеКС.Счетчик + 1;
		СтрокаОписания = КонтекстПроверки.ТаблицаОписаний.Добавить();
		ЗаполнитьЗначенияСвойств(СтрокаОписания, КонтекстПроверки.НастройкиКС);
		СтрокаОписания.НомерЭлемента = КонтекстПроверки.ДанныеКС.Счетчик;
	КонецЕсли;
	
КонецПроцедуры

Функция СформироватьПоказатели(КонтекстПроверки, ИменаПоказателей)
	
	Показатели = Новый Структура;
	
	Для каждого Элемент Из ИменаПоказателей Цикл
		
		Показатели.Вставить(Элемент.Ключ, Новый Структура());
		
		МассивПредставления		= Новый Массив;
		ИмяРаздела				= "";
		ПредставлениеРаздела	= "";
		ПрефиксРаздела			= Сред(Элемент.Ключ, 1, 2);
		
		КонтекстПроверки.ДанныеКС.ИменаРазделов.Свойство(ПрефиксРаздела			, ИмяРаздела);
		КонтекстПроверки.ДанныеКС.ПредставленияРазделов.Свойство(ПрефиксРаздела	, ПредставлениеРаздела);
		
		МассивПредставления.Добавить(КонтекстПроверки.ДанныеКС.ПредставленияПоказателей.Получить(Элемент.Значение));
		МассивПредставления.Добавить(ПредставлениеРаздела);
		
		Представление		= СтрСоединить(МассивПредставления, " ");
		Многострочный		= Ложь;
		Округление			= Ложь;
		
		// постоянные данные
		Показатели[Элемент.Ключ].Вставить("ПрефиксРаздела"	, ПрефиксРаздела);
		Показатели[Элемент.Ключ].Вставить("ИмяПоказателя"	, Элемент.Значение);
		Показатели[Элемент.Ключ].Вставить("Представление"	, Представление);
		Показатели[Элемент.Ключ].Вставить("ИмяРаздела"		, ИмяРаздела);
		
		// переменные данные
		Показатели[Элемент.Ключ].Вставить("Отчет");
		Показатели[Элемент.Ключ].Вставить("НомерСтраницы"	, "");
		Показатели[Элемент.Ключ].Вставить("Значение");
		Показатели[Элемент.Ключ].Вставить("ЗначениеСтр"		, "");
		Показатели[Элемент.Ключ].Вставить("Итог"			, 0);
		
		// сервисные данные
		Показатели[Элемент.Ключ].Вставить("Многострочный"	, Многострочный);
		Показатели[Элемент.Ключ].Вставить("Округление"		, Округление);
		Показатели[Элемент.Ключ].Вставить("Синоним"			, Элемент.Ключ);
		Показатели[Элемент.Ключ].Вставить("Дополнение"		, "");
		Показатели[Элемент.Ключ].Вставить("Заполнять"		, Истина);
		Показатели[Элемент.Ключ].Вставить("Расшифровывать"	, Истина);
		Показатели[Элемент.Ключ].Вставить("Инверсия"		, Ложь);
		
	КонецЦикла;
	
	Возврат Показатели;
	
КонецФункции

Процедура ЗарегистрироватьПроверку(КонтекстПроверки)
	КонтекстПроверки.ДанныеКС.Счетчик = КонтекстПроверки.ДанныеКС.Счетчик + 1;
