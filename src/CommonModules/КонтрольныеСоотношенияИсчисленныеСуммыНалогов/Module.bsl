Процедура ПроверкаКС_2_01(КонтекстПроверки)
	
	Заголовки = Новый Структура;
	Заголовки.Вставить("Раздел"				, НСтр("ru='Сведения о суммах налогов'"));
	Заголовки.Вставить("НомерКС"			, НСтр("ru='2.1'"));
	Заголовки.Вставить("Описание"			, 
		НСтр("ru='Если КБК в стр.3 = '") + КонтекстПроверки.ДанныеКС.КБК.Прибыль_Ин + НСтр("ru=', стр.5 = 21/01, то '") +
		НСтр("ru='дата представления уведомления <= 25.02'"));
	Заголовки.Вставить("ШаблонДетализации"	, НСтр("ru='%1 <= 25.02'"));
	Заголовки.Вставить("Нарушение"			, НСтр("ru='Возможно указана ошибочная дата подписи отчета'"));
	Заголовки.Вставить("Рекомендации"		, "Уточните дату подписи на Титульном листе");
	Заголовки.Вставить("Основание"			, НСтр("ru='Письмо ФНС России от 21.02.2023 № ЕА-4-15/2048@'"));
	Заголовки.Вставить("ШаблонДопСведений"	, НСтр("ru='Страница %1; Проверка носит справочный характер'"));
	ПодготовитьНастройки(КонтекстПроверки, Заголовки);
	
	Если КонтекстПроверки.Документирование Тогда
		Возврат;
	КонецЕсли;
	
	КонтекстПроверки.НастройкиКС.Вставить("КБК"				, КонтекстПроверки.ДанныеКС.КБК.Прибыль_Ин);
	КонтекстПроверки.НастройкиКС.Вставить("КодПериода"		, "21/01");
	КонтекстПроверки.НастройкиКС.Вставить("ДатаПодписи"		, "0225");
	
	ПроверкаКС_2_01_код(КонтекстПроверки);
	
КонецПроцедуры

Процедура ПроверкаКС_2_01_код(КонтекстПроверки)
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("Р1С01", "КБК");
	ИменаПоказателей.Вставить("Р1С02", "Период");
	ИменаПоказателей.Вставить("Р1С03", "НомерМесКварт");
	ИменаПоказателей.Вставить("Р0С04", "ДАТА_ПОДПИСИ");
	
	Показатели = СформироватьПоказатели(КонтекстПроверки, ИменаПоказателей);
	
	Для каждого СтраницаРаздела Из КонтекстПроверки.ДанныеКС.Р1 Цикл
		
		ЗарегистрироватьПроверку(КонтекстПроверки);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р1С01, СтраницаРаздела);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р1С02, СтраницаРаздела);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р1С03, СтраницаРаздела);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р0С04, КонтекстПроверки.ДанныеКС.Р0);
	
		КонтекстПроверки.НастройкиКС.ЭтоОшибка = Показатели.Р1С01.Значение = КонтекстПроверки.НастройкиКС.КБК
			И СтраницаРаздела.КодПериода = КонтекстПроверки.НастройкиКС.КодПериода
			И КонтекстПроверки.ДанныеКС.Р0.ДатаПодписи2 > КонтекстПроверки.НастройкиКС.ДатаПодписи;
		КонтекстПроверки.НастройкиКС.ДопСведения = СтрШаблон(КонтекстПроверки.НастройкиКС.ШаблонДопСведений,
			Сред(СтраницаРаздела.НомерСтраницы, 5));
		КонтекстПроверки.НастройкиКС.Детализация = СтрШаблон(КонтекстПроверки.НастройкиКС.ШаблонДетализации, 
			КонтекстПроверки.ДанныеКС.Р0.ДатаПодписи3);
		ДополнитьТаблицуРезультата(КонтекстПроверки);
		
	КонецЦикла;
	
КонецПроцедуры

