// Выполняет запуск обмена с сайтом из регламентного задания.
//
// Параметры:
//	КодНастройкиИнтеграции		- строка с кодом узла плана обмена.
//
Процедура ЗаданиеВыполнитьОбмен(КодНастройкиИнтеграции, ЭтоРеглЗадание = Ложь) Экспорт

	ОбщегоНазначения.ПриНачалеВыполненияРегламентногоЗадания(
	Метаданные.РегламентныеЗадания.ИнтеграцияСИнтернетМагазином);
	
	НастройкаИнтеграции = Справочники.НастройкиИнтеграцииСИнтернетМагазином.ПолучитьСсылку(Новый УникальныйИдентификатор(КодНастройкиИнтеграции));
	Параметры = Неопределено;
	
	Если НЕ ЗначениеЗаполнено(НастройкаИнтеграции) Тогда
	
		ТекстОшибки = НСтр("ru = 'Не найдена настройка с кодом'") + " " + КодНастройкиИнтеграции;
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		ТекстОшибки);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( , ТекстОшибки, Истина );
		
		Возврат;
		
	КонецЕсли;
	
	ДанныеНастройкиИнтеграции = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(НастройкаИнтеграции,
	"ПометкаУдаления,ВыгружатьНаСайт,ВыполнятьПолнуюВыгрузкуПринудительно,ОбменТоварами,ОбменЗаказами,
	|ИспользоватьРегламентныеЗадания,ИдентификаторРегламентногоЗадания,
	|ИспользоватьПолныйОбмен,РасписаниеРегламентногоЗаданияПолныйОбмен,");
	
	Если ЭтоРеглЗадание = Истина И
		(ДанныеНастройкиИнтеграции.ПометкаУдаления Или НЕ ДанныеНастройкиИнтеграции.ИспользоватьРегламентныеЗадания) Тогда
		
		ТекстОшибки = НСтр("ru = 'Использование регламентного заданания отключено. Обмен отменен.'" );
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		ТекстОшибки 
		);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( НастройкаИнтеграции, ТекстОшибки, Истина );
		
		Если ЗначениеЗаполнено(ДанныеНастройкиИнтеграции.ИдентификаторРегламентногоЗадания) Тогда
			
			УдалитьРегламентноеЗаданиеОбмена(НастройкаИнтеграции);
			НастройкаИнтеграцииРегламентныеЗаданияОтключить( НастройкаИнтеграции );
			
		КонецЕсли;
		
		Возврат;
		
	КонецЕсли;
	
	ТекстСообщения = НСтр("ru = 'Начало выполнение регламентного заданания обмена с сайтом.'" );
	ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
	УровеньЖурналаРегистрации.Информация,
	НастройкаИнтеграции.Метаданные(),
	НастройкаИнтеграции,
	ТекстСообщения 
	);
	
	Попытка
		
		ВыгружатьТолькоИзменения = НЕ ДанныеНастройкиИнтеграции.ВыполнятьПолнуюВыгрузкуПринудительно;
		
		Если ВыгружатьТолькоИзменения И ДанныеНастройкиИнтеграции.ИспользоватьПолныйОбмен И
			ЗначениеЗаполнено(ДанныеНастройкиИнтеграции.РасписаниеРегламентногоЗаданияПолныйОбмен) Тогда
			
			Попытка
				РасписаниеЗадания = ЗначениеИзСтрокиВнутр(ДанныеНастройкиИнтеграции.РасписаниеРегламентногоЗаданияПолныйОбмен);
				ДатаЗавершенияПолногоОбмена = ХранилищеОбщихНастроек.Загрузить("ИнтеграцияСИнтернетМагазином/" + КодНастройкиИнтеграции,"ДатаЗавершенияПолногоОбмена");
				Если ДатаЗавершенияПолногоОбмена = Неопределено Тогда
					ДатаЗавершенияПолногоОбмена = Дата(01,01,01);
				КонецЕсли;
				Если РасписаниеЗадания.ТребуетсяВыполнение(ТекущаяДатаСеанса(), ДатаЗавершенияПолногоОбмена, ДатаЗавершенияПолногоОбмена) Тогда
					ВыгружатьТолькоИзменения = Ложь;
				КонецЕсли;
				
			Исключение
				РасписаниеЗадания = Неопределено;
			КонецПопытки;
			
		КонецЕсли;
		
		ВыполнитьОбмен(НастройкаИнтеграции, Параметры, 
		НСтр("ru = 'Фоновый обмен'"), ВыгружатьТолькоИзменения, ЭтоРеглЗадание);
		
		Если ЭтоРеглЗадание = Ложь Тогда
			СоздатьОбновитьРегламентноеЗаданиеОбмена(НастройкаИнтеграции, Истина);
		КонецЕсли;
		
	Исключение
		
		ТекстОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		ТекстОшибки
		);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, НастройкаИнтеграции, Истина );
		
		СтрокаТаблицыИнформации = Параметры.ТаблицаИнформации[ Параметры.ТаблицаИнформации.Количество() - 1 ];
		СтрокаТаблицыИнформации.Описание = СтрокаТаблицыИнформации.Описание + Символы.ПС + ТекстОшибки;
		ТаблицыИнформацииЗафиксироватьЗавершениеВыгрузки(СтрокаТаблицыИнформации, Истина, НСтр("ru = 'Обмен отменен.'"));
	
		Если ЭтоРеглЗадание = Истина Тогда
			ДобавитьОшибкуВСчетчикРегламентногоЗаданияИОтключитьПриПревышенииСУведомлением(НастройкаИнтеграции, ТекущаяДатаСеанса(), ТекстОшибки);
		КонецЕсли;
		
	КонецПопытки;
	
	ВыполнитьДействияПриЗавершенииОбмена( Параметры );
	
	ТекстСообщения = НСтр("ru = 'Окончание выполнения регламентного заданания обмена с сайтом.'" );
	ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
	УровеньЖурналаРегистрации.Информация,
	НастройкаИнтеграции.Метаданные(),
	НастройкаИнтеграции,
	ТекстСообщения 
	);
	
КонецПроцедуры

Функция МаксимальноеКоличествоОшибокДляОтключенияРегламентногоЗадания()
	Возврат 200;
КонецФункции

Процедура ДобавитьОшибкуВСчетчикРегламентногоЗаданияИОтключитьПриПревышенииСУведомлением(НастройкаИнтеграции, ДатаИнтеграции, ТекстОшибки)

	МЗ = РегистрыСведений.СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.СоздатьМенеджерЗаписи();
	МЗ.НастройкаИнтеграции = НастройкаИнтеграции;
	МЗ.ДатаНачала = ДатаИнтеграции;
	МЗ.ТекстОшибки = ТекстОшибки;
	МЗ.Записать();
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	ЕСТЬNULL(КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.ДатаНачала), 0) КАК КоличествоОшибок
		|ИЗ
		|	РегистрСведений.СчетчикОшибокРегламентногоЗаданияИнтернетМагазина КАК СчетчикОшибокРегламентногоЗаданияИнтернетМагазина
		|ГДЕ
		|	СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.НастройкаИнтеграции = &НастройкаИнтеграции
		|	И (ВЫРАЗИТЬ(СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.ТекстОшибки КАК СТРОКА(1))) <> """"";
	
	Запрос.УстановитьПараметр("НастройкаИнтеграции", НастройкаИнтеграции);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	ВыборкаДетальныеЗаписи.Следующий();
	
	Если ВыборкаДетальныеЗаписи.КоличествоОшибок < МаксимальноеКоличествоОшибокДляОтключенияРегламентногоЗадания() Тогда
		Возврат;
	КонецЕсли;
	
	УдалитьРегламентноеЗаданиеОбмена(НастройкаИнтеграции);
	НастройкаИнтеграцииРегламентныеЗаданияОтключить( НастройкаИнтеграции );
	
	ТемуИТелоПисьма = ПолучитьТемуИТелоПисьмаОтключенияОбмена(НастройкаИнтеграции);
	СоздатьОтправитьЭлектронноеПисьмо(НастройкаИнтеграции, ТемуИТелоПисьма.ТемаПисьма, ТемуИТелоПисьма.ТелоПисьма);
	
	СчетчикОшибокРегламентногоЗаданияОчистить( НастройкаИнтеграции );
		
КонецПроцедуры

Процедура СчетчикОшибокРегламентногоЗаданияОчистить( НастройкаИнтеграции )
	НЗ = РегистрыСведений.СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.СоздатьНаборЗаписей();
	НЗ.Отбор.НастройкаИнтеграции.Установить(НастройкаИнтеграции);
	НЗ.Записать();
КонецПроцедуры

Функция ПолучитьТемуИТелоПисьмаОтключенияОбмена(НастройкаИнтеграции)
	
	ДанныеНастройкиИнтеграции = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(НастройкаИнтеграции, "Наименование");
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ ПЕРВЫЕ 3
		|	СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.ДатаНачала КАК ДатаНачала,
		|	СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.ТекстОшибки КАК ТекстОшибки
		|ИЗ
		|	РегистрСведений.СчетчикОшибокРегламентногоЗаданияИнтернетМагазина КАК СчетчикОшибокРегламентногоЗаданияИнтернетМагазина
		|ГДЕ
		|	СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.НастройкаИнтеграции = &НастройкаИнтеграции
		|	И (ВЫРАЗИТЬ(СчетчикОшибокРегламентногоЗаданияИнтернетМагазина.ТекстОшибки КАК СТРОКА(1))) <> """"
		|
		|УПОРЯДОЧИТЬ ПО
		|	ДатаНачала УБЫВ";
	
	Запрос.УстановитьПараметр("НастройкаИнтеграции", НастройкаИнтеграции);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	МассивОшибок = Новый Массив;
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		МассивОшибок.Добавить(СтрШаблон("%1 - %2", ВыборкаДетальныеЗаписи.ДатаНачала, ВыборкаДетальныеЗаписи.ТекстОшибки));
	КонецЦикла;
	
	МаксимальноеКоличествоОшибок = МаксимальноеКоличествоОшибокДляОтключенияРегламентногоЗадания();
	
	ТелоПисьма = НСтр(СтрШаблон("ru = 'Интеграция с интернет-магазином %1 отменена.
	|Причина: Более " + МаксимальноеКоличествоОшибок + " ошибок подряд при обмене с интернет-магазином.
	|	Автоматический обмен отключен.
	|	Устраните ошибки обмена из списка ниже.
	|	После исправления ошибок включите автоматический обмен на вкладке ""Расписание"".
	|
	|Список ошибок:
	|	%2'", ДанныеНастройкиИнтеграции.Наименование, СтрСоединить(МассивОшибок,";" + Символы.ПС)));
	
	Результат = Новый Структура;
	Результат.Вставить("ТемаПисьма", НСтр("ru = 'Интеграция с интернет-магазином. Обмен отменен.'"));
	Результат.Вставить("ТелоПисьма", ТелоПисьма);
	
	Возврат Результат;
	
КонецФункции

Процедура СоздатьОтправитьЭлектронноеПисьмо(НастройкаИнтеграции, ТемаПисьма, ТелоПисьма)
	
	Попытка
		
		ДанныеНастройкиИнтеграции = ОбщегоНазначения.ЗначенияРеквизитовОбъекта(НастройкаИнтеграции,
		"ПодпискиНаСобытие,УчетнаяЗаписьДляОповещения,ОтправлятьСкрытуюКопиюВПоддержку,ИспользоватьОповещения");
		
		Если НЕ ДанныеНастройкиИнтеграции.ИспользоватьОповещения
			Или Не ЗначениеЗаполнено(ДанныеНастройкиИнтеграции.УчетнаяЗаписьДляОповещения) Тогда
			Возврат;
		КонецЕсли;
		
		ДлительныеОперации.СообщитьПрогресс( , "Отправка письма с текстом ошибки ... " + Формат( ТекущаяДатаСеанса(), "ДФ=HH:mm:ss" ) );
		
		ЗначенияЗаполнения = Новый Структура;
		ЗначенияЗаполнения.Вставить("ТипСобытия", Перечисления.ТипыСобытий.ЭлектронноеПисьмо);
		
		НовоеСобытие = Документы.Событие.СоздатьДокумент();
		НовоеСобытие.Заполнить(ЗначенияЗаполнения);
		НовоеСобытие.Дата = ТекущаяДатаСеанса();
		НовоеСобытие.УчетнаяЗапись = ДанныеНастройкиИнтеграции.УчетнаяЗаписьДляОповещения;
		НовоеСобытие.ТипСобытия = ПредопределенноеЗначение("Перечисление.ТипыСобытий.ЭлектронноеПисьмо");
		НовоеСобытие.Тема = ТемаПисьма;
		НовоеСобытие.СодержаниеHTML = СтрЗаменить( ТелоПисьма, Символы.ПС, "<br>" );
		НовоеСобытие.Содержание = ТелоПисьма;
		
		НовоеСобытие.Участники.Очистить();
		
		Для Каждого Стр Из ДанныеНастройкиИнтеграции.ПодпискиНаСобытие.Выгрузить() Цикл
			
			НоваяСтрока = НовоеСобытие.Участники.Добавить();
			НоваяСтрока.Контакт = Стр.ЭлектроннаяПочта;
			НоваяСтрока.КакСвязаться = Стр.ЭлектроннаяПочта;
			НоваяСтрока.ТипПолучателяЭлектронногоПисьма
			= ПредопределенноеЗначение("Перечисление.ТипыПолучателейЭлектронногоПисьма.Кому");
			
		КонецЦикла;
		
		Если ДанныеНастройкиИнтеграции.ОтправлятьСкрытуюКопиюВПоддержку Тогда
			НоваяСтрока = НовоеСобытие.Участники.Добавить();
			НоваяСтрока.Контакт = НСтр( "ru = 'Команда разработки приложения <sbm@1c.ru>'" );
			НоваяСтрока.КакСвязаться = "sbm@1c.ru";
			НоваяСтрока.ТипПолучателяЭлектронногоПисьма
			= ПредопределенноеЗначение("Перечисление.ТипыПолучателейЭлектронногоПисьма.СкрытаяКопия");
		КонецЕсли;
		
		Если НовоеСобытие.Участники.Количество() = 0 Тогда
			Возврат;
		КонецЕсли;
		
		НовоеСобытие.Записать();
		
		НовоеСобытие.ОтправитьЭлектронноеПисьмо();
		
	Исключение
	
		ТекстОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		НСтр(СтрШаблон("ru = 'Ошибка отправки электронного письма.
		| %1'", ТекстОшибки))
		);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, НастройкаИнтеграции, Истина );
	КонецПопытки;
	
КонецПроцедуры

// Выполняет запуск интерактивного обмена с сайтом.
//
// Параметры:
//   ПараметрыОбмена - структура с параметрами обмена с сайтом
//	- УзелОбмена		- ПланОбменаСсылка.ОбменУправлениеНебольшойФирмойСайт,
//	- ВыгружатьТолькоИзменения - булево, влияет на объем выгружаемых данных.
//
Процедура ВыполнитьОбменИнтерактивно(ПараметрыОбмена, АдресРезультата = Неопределено) Экспорт
	
	НастройкаИнтеграции = ПараметрыОбмена.НастройкаИнтеграции;
	
	Параметры = Неопределено;
	Попытка
		
		ВыполнитьОбмен(НастройкаИнтеграции, Параметры,
		НСтр("ru = 'Интерактивный обмен'"),
		ПараметрыОбмена.ВыгружатьТолькоИзменения
		);
		
		СоздатьОбновитьРегламентноеЗаданиеОбмена(НастройкаИнтеграции, Истина);
		
	Исключение
		
		ИнформацияОбОшибке = ИнформацияОбОшибке();
		ТекстОшибки = ПодробноеПредставлениеОшибки( ИнформацияОбОшибке );
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		ТекстОшибки
		);
        
        ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, НастройкаИнтеграции, Истина );
        
		
		СтрокаТаблицыИнформации = Параметры.ТаблицаИнформации[ Параметры.ТаблицаИнформации.Количество() - 1 ];
		СтрокаТаблицыИнформации.Описание = СтрокаТаблицыИнформации.Описание + Символы.ПС + ТекстОшибки;
		ТаблицыИнформацииЗафиксироватьЗавершениеВыгрузки(СтрокаТаблицыИнформации, Истина, НСтр("ru = 'Обмен отменен.'"));
		
	КонецПопытки;
	
	ВыполнитьДействияПриЗавершенииОбмена( Параметры );
	
КонецПроцедуры

// Выполняет обмен с сайтом.
//
// Параметры:
//	НастройкаИнтеграции		- СправочникСсылка.НастройкиИнтеграции,
//	РежимЗапускаОбмена - строка, имя события для ЖурналаРегистрации
//	ВыгружатьТолькоИзменения - булево, влияет на объем выгружаемых данных.
//
Процедура ВыполнитьОбмен( НастройкаИнтеграции, Параметры, РежимЗапускаОбмена, ВыгружатьТолькоИзменения = Истина, ЭтоРеглЗадание = Ложь ) 
	
	Если НЕ ЗначениеЗаполнено(НастройкаИнтеграции) Тогда
		Возврат;
	КонецЕсли;

	Время001 = ТекущаяУниверсальнаяДатаВМиллисекундах();
	ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( "Начало выполнения обмена данными", НастройкаИнтеграции );
	
	
	Параметры = ПолучитьСтруктуруОсновныхПараметровОбмена();
	
	Параметры.Вставить("ЭтоРеглЗадание", ЭтоРеглЗадание);
	Параметры.Вставить("ОбменЧерезВебСервис", Ложь);
	Параметры.Вставить("РежимЗапускаОбмена", РежимЗапускаОбмена);
	Параметры.Вставить("ВыгружатьТолькоИзменения", ВыгружатьТолькоИзменения);
	Параметры.Вставить("СоответствиеФайлов", Новый Соответствие);
	
	СистемнаяИнформация = Новый СистемнаяИнформация;
	ПлатформаWindows = СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86
	ИЛИ СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86_64;
	
	Параметры.Вставить("ПлатформаWindows", ПлатформаWindows);
	
	ДобавитьНастройкиВПараметры(НастройкаИнтеграции, Параметры);
	
	Параметры.Вставить("ТаблицаИнформации", НоваяТаблицаИнформации());

	// тестовая проверка авторизации
	Если Параметры.ВыгружатьНаСайт Тогда
		РезультатПодключения = ВыполнитьТестовоеПодключениеКСайту(НастройкаИнтеграции);
		Если РезультатПодключения.Результат = Ложь Тогда
			// ошибка авторизации
			ТекстОшибки = РезультатПодключения.Текст;
			СтрокаТаблицыИнформации = Параметры.ТаблицаИнформации.Добавить();
			СтрокаТаблицыИнформации.Описание = СтрокаТаблицыИнформации.Описание + Символы.ПС + ТекстОшибки;
			СтрокаТаблицыИнформации.ДействиеПриОбмене = Перечисления.ДействияПриОбмене.ВыгрузкаДанных;
			ТаблицыИнформацииЗафиксироватьЗавершениеВыгрузки(СтрокаТаблицыИнформации, Истина, НСтр("ru = 'Обмен отменен.'"));
			
			Возврат;
		КонецЕсли;
	КонецЕсли;

	
	Если Параметры.ВыгружатьНаСайт Тогда
		
		Параметры.ОписаниеОшибки = "";
		
		НастройкиПодключения = ПолучитьНастройкиПодключенияНовый(НастройкаИнтеграции);
		ДобавитьПараметрыПротоколаОбменаВСтруктуру(НастройкиПодключения);
		
		Параметры.Вставить("НастройкиПодключения", НастройкиПодключения);
		
	КонецЕсли;
	
	КаталогВыгрузки = Параметры.КаталогВыгрузки;
	Если ПустаяСтрока(КаталогВыгрузки) ИЛИ Параметры.ВыгружатьНаСайт Тогда
		
		КаталогВыгрузки = КаталогВременныхФайлов();
		
	Иначе
		
		КаталогВыгрузки = Параметры.КаталогВыгрузки;
		ПоследнийСимвол = Прав(КаталогВыгрузки, 1);
		
		Если НЕ ПоследнийСимвол = "\" Тогда
			КаталогВыгрузки = КаталогВыгрузки + "\";
		КонецЕсли;
	КонецЕсли;
	
	ПодкаталогБезопасностиКаталогаВыгрузки = "webdata - " + НастройкаИнтеграции.УникальныйИдентификатор();
	
	Если Параметры.ВыгружатьНаСайт Тогда
		
		// Находим предыдущие каталоги и удаляем старые
		ТекущаяУниверсальнаяДатаВМиллисекундах = ТекущаяУниверсальнаяДатаВМиллисекундах();
		СрокХраненияСтарыхКаталогов = 1800000; // 30 мин.
		СрокХраненияСтарыхКаталоговПолныйОбмен = 10800000; // 3 часа.
		МассивНайденныхФайлов = НайтиФайлы(КаталогВыгрузки, ПодкаталогБезопасностиКаталогаВыгрузки + "_*", Ложь);
		Для Каждого СтрФайл Из МассивНайденныхФайлов Цикл
			
			Если НЕ СтрФайл.ЭтоКаталог() Тогда
				Продолжить;
			КонецЕсли;
			
			УниверсальнаяДатаВМиллисекундахСтрокой = СтрЗаменить(СтрЗаменить(СтрФайл.ИмяБезРасширения,
			"_full", ""),ПодкаталогБезопасностиКаталогаВыгрузки + "_", "");
			
			Попытка
				
				ОписаниеТипа = Новый ОписаниеТипов("Число");
				УниверсальнаяДатаВМиллисекундах = ОписаниеТипа.ПривестиЗначение(УниверсальнаяДатаВМиллисекундахСтрокой);
				
				Если ВыгружатьТолькоИзменения = Истина И СтрНайти(СтрФайл.ИмяБезРасширения, "_full") <> 0 И
					УниверсальнаяДатаВМиллисекундах > (ТекущаяУниверсальнаяДатаВМиллисекундах - СрокХраненияСтарыхКаталоговПолныйОбмен) Тогда
					Продолжить;
				КонецЕсли;
				
				Если УниверсальнаяДатаВМиллисекундах <= (ТекущаяУниверсальнаяДатаВМиллисекундах - СрокХраненияСтарыхКаталогов) Тогда
					УдалитьФайлы(СтрФайл.ПолноеИмя);
				КонецЕсли;
				
			Исключение
				
				ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
				
				ОписаниеИсключительнойОшибки = ОписаниеИсключительнойОшибки(ПодробноеПредставлениеОшибки, НСтр("ru = 'Не удалось очистить каталог обмена, п1:'") + 
				" (" + СтрФайл.ИмяБезРасширения + ")");
				
				Параметры.ОписаниеОшибки = "";
				ДобавитьОписаниеОшибки(Параметры.ОписаниеОшибки, ОписаниеИсключительнойОшибки);
				
				ЗаписатьВТаблицуИнформации(Параметры.ОписаниеОшибки, Параметры.ТаблицаИнформации, Истина);
				
			КонецПопытки;
		КонецЦикла;
		
		Если ВыгружатьТолькоИзменения = Ложь Тогда
			ПодкаталогБезопасностиКаталогаВыгрузки = ПодкаталогБезопасностиКаталогаВыгрузки + "_full"
		КонецЕсли;
		
		ПодкаталогБезопасностиКаталогаВыгрузки = ПодкаталогБезопасностиКаталогаВыгрузки +
		"_" + XMLСтрока(ТекущаяУниверсальнаяДатаВМиллисекундах);
		
	КонецЕсли;
	КаталогНаДиске = КаталогВыгрузки + ПодкаталогБезопасностиКаталогаВыгрузки;
	КаталогНаДиске = ПодготовитьПутьДляПлатформы(ПлатформаWindows, КаталогНаДиске);
	
	Попытка
		
		СоздатьКаталог(КаталогНаДиске);
		
	Исключение
		
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ЗаписатьВТаблицуИнформации(ОписаниеИсключительнойОшибки(ПодробноеПредставлениеОшибки), Параметры.ТаблицаИнформации, Истина);
		
		Возврат;
	КонецПопытки;
	
	Параметры.ОписаниеОшибки = "";
	Если НЕ ОчиститьКаталог(КаталогНаДиске, Параметры.ОписаниеОшибки) Тогда
		
		ЗаписатьВТаблицуИнформации(Параметры.ОписаниеОшибки, Параметры.ТаблицаИнформации, Истина);
		
		Возврат;
	КонецЕсли;
	
	ФайлЗагрузки = Параметры.ФайлЗагрузки;
	ФайлЗагрузки = ПодготовитьПутьДляПлатформы(ПлатформаWindows, ФайлЗагрузки);
	Параметры.Вставить("ФайлЗагрузки", ФайлЗагрузки);
	
	ПланыОбменаРегистрацииИзмененийЗаполнитьНомерСообщения( Параметры );
	
	ПолучитьСтруктуруИзменений(Параметры);
	
	ТоварыУспешноВыгружены = Истина;
	
	Параметры.Вставить("КаталогНаДиске", КаталогНаДиске);
	Параметры.Вставить("ПодкаталогФайлов", "import_files");
	Параметры.Вставить("ИндексФайлаОбмена", 0);
	
	
	Если Параметры.ОбменТоварами Тогда 
		
		ДлительныеОперации.СообщитьПрогресс( , "Выгрузка номенклатуры ... " + Формат( ТекущаяДатаСеанса(), "ДФ=HH:mm:ss" ) );
		
		ТоварыУспешноВыгружены = ВыгрузитьНоменклатуру(Параметры);
		
		Если ТоварыУспешноВыгружены Тогда
			УдалитьРегистрациюИзменений(НастройкаИнтеграции, Параметры, Истина, Ложь);
		КонецЕсли;
		
		Если ТоварыУспешноВыгружены Тогда
			
			ЗаписатьДанныеОВыгрузкеТоваров(НастройкаИнтеграции, Параметры);
			
		КонецЕсли;
		
	КонецЕсли;
	
	ЗаказыУспешноВыгружены = Истина;
	
	Если Параметры.ОбменЗаказами Тогда
		
		ДлительныеОперации.СообщитьПрогресс( , "Обмен заказами ... " + Формат( ТекущаяДатаСеанса(), "ДФ=HH:mm:ss" ) );
		
		ЗаказыУспешноВыгружены = ВыполнитьОбменЗаказами(Параметры, Параметры.ТаблицаИнформации);
		
		Если ЗаказыУспешноВыгружены Тогда
			УдалитьРегистрациюИзменений(НастройкаИнтеграции, Параметры, Ложь, Истина);
		КонецЕсли;
		
	КонецЕсли;
	
	ЗаписьНаУслугиУспешноЗагружены = Истина;
	Если Параметры.ОбменЗаписьНаУслуги Тогда
		Параметры.Вставить("РежимВыгрузкиУслуг", Истина);
		
		ДлительныеОперации.СообщитьПрогресс( , "Обмен записями на услуги ... " + Формат( ТекущаяДатаСеанса(), "ДФ=HH:mm:ss" ) );
		
		ЗаписьНаУслугиУспешноЗагружены = ВыполнитьОбменЗаписьНаУслуги(Параметры, Параметры.ТаблицаИнформации);
		
		ГрафикУспешноВыгружен = ВыгрузитьУслуги(Параметры);
		
		Если ЗаписьНаУслугиУспешноЗагружены И ГрафикУспешноВыгружен Тогда
			УдалитьРегистрациюИзменений(НастройкаИнтеграции, Параметры, Ложь, Ложь, Истина);
		КонецЕсли;
		
	КонецЕсли;
	
	Если Параметры.ВыполнятьПолнуюВыгрузкуПринудительно
		И ТоварыУспешноВыгружены Тогда
		
		УзелОбъект = НастройкаИнтеграции.ПолучитьОбъект();
		УзелОбъект.ВыполнятьПолнуюВыгрузкуПринудительно = Ложь;
		УзелОбъект.Записать();
		
	КонецЕсли;
	
	Если НЕ Параметры.ВыгружатьТолькоИзменения Тогда
		ХранилищеОбщихНастроек.Сохранить("ИнтеграцияСИнтернетМагазином/" + НастройкаИнтеграции.УникальныйИдентификатор(),"ДатаЗавершенияПолногоОбмена", ТекущаяДатаСеанса());
	КонецЕсли;
	
	Если ТоварыУспешноВыгружены = Истина 
	И Параметры.ВыгружатьТолькоИзменения = Ложь 
	И НастройкаИнтеграции.НеобходимоВыполнитьПолныйОбмен = Истина Тогда
		НастройкаИнтеграцииОбъект = НастройкаИнтеграции.ПолучитьОбъект();
		НастройкаИнтеграцииОбъект.НеобходимоВыполнитьПолныйОбмен = Ложь;
		НастройкаИнтеграцииОбъект.Записать();
	КонецЕсли;

	ДлительныеОперации.СообщитьПрогресс( , "Обмен выполнен. " + Формат( ТекущаяДатаСеанса(), "ДФ=HH:mm:ss" ) );
	
	Время002 = ТекущаяУниверсальнаяДатаВМиллисекундах();
	ДлительностьСекунд = (Время002 - Время001)/1000;
	ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( "Окончание выполнения обмена данными. Длительность " + ДлительностьСекунд + " с." , НастройкаИнтеграции );
	
КонецПроцедуры

Функция ПолучитьФайлыОбмена(НастройкаИнтеграции, ВыгружатьТолькоИзменения = Истина, ПараметрыКонтекст = Неопределено) Экспорт
	
	Параметры = ПолучитьСтруктуруОсновныхПараметровОбмена();
	
	Параметры.Вставить("ОбменЧерезВебСервис", Ложь);
	Параметры.Вставить("ВыгружатьТолькоИзменения", ВыгружатьТолькоИзменения);
	
	ПакетыОбмена = Новый Соответствие;
	
	ДобавитьНастройкиВПараметры(НастройкаИнтеграции, Параметры);
	
	СистемнаяИнформация = Новый СистемнаяИнформация;
	ПлатформаWindows = СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86
	ИЛИ СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86_64;
	
	КаталогНаДиске = КаталогВременныхФайлов();
	КаталогНаДиске = ПодготовитьПутьДляПлатформы(ПлатформаWindows, КаталогНаДиске);
	
	Параметры.ВыгружатьКартинки = Истина;
	Параметры.Вставить("ПлатформаWindows", ПлатформаWindows);
	Параметры.Вставить("КаталогНаДиске", КаталогНаДиске);
	Параметры.Вставить("ПодкаталогФайлов", "import_files");
	Параметры.Вставить("ИндексФайлаОбмена", 0);
	
	ТаблицаКаталогов = ПодготовитьТаблицуКаталогов(Параметры);
	ЗагрузитьВидыЦенВМассив(Параметры);
	
	ПолучитьСтруктуруИзменений(Параметры);
	
	Если Параметры.ОбменТоварами Тогда 
		
		Для Каждого СтрокаТаблицыКаталогов Из ТаблицаКаталогов Цикл
			
			Параметры.Вставить("МассивВыгруженныхНоменклатур", Новый Массив);
			
			СтрокаТаблицыКаталогов.СтруктураРезультата = Новый Структура("ВыгруженоТоваров,ВыгруженоКартинок,ВыгруженоФайлов,ВыгруженоПредложений,ОписаниеОшибки", 0, 0, 0, 0, "");
			Параметры.Вставить("СтрокаТаблицыКаталогов", СтрокаТаблицыКаталогов);
			Параметры.Вставить("РучнаяНастройкаКаталога", СтрокаТаблицыКаталогов.РучнаяНастройкаКаталога);
			Если СтрокаТаблицыКаталогов.РучнаяНастройкаКаталога > 0 Тогда
				Параметры.Вставить("ОтборНоменклатураРучнаяНастройкаКаталога", 
				ОтборНоменклатураРучнаяНастройкаКаталога(Параметры.НастройкаИнтеграции, Параметры.СтрокаТаблицыКаталогов.ИдентификаторКаталога, СтрокаТаблицыКаталогов.РучнаяНастройкаКаталога));
			Иначе
				Параметры.Вставить("ОтборНоменклатураРучнаяНастройкаКаталога", Неопределено);
			КонецЕсли;
			
			ПодготовитьДанныеДляВыгрузки(Параметры);
			
			Если Параметры.ВыборкаНоменклатуры.Количество() = 0 Тогда
				// Если нет данных по номенклатуре, не формируем XDTO-объекты.
				Продолжить;
			КонецЕсли;
			
			Параметры.ИндексФайлаОбмена = 0;
			Параметры.НомерВВыборке = Параметры.ВыборкаНоменклатуры.Количество();
			URIПространстваИмен = "urn:1C.ru:commerceml_210";
			ПакетCML = ФабрикаXDTO.Пакеты.Получить(URIПространстваИмен);
			
			Пока Параметры.НомерВВыборке > 0 Цикл
				// Выгрузить Классификатор И Каталог
				КоммерческаяИнформацияXDTO = ПодготовитьДанныеXDTOКлассификаторИКаталог(Параметры, ПакетCML);
				ПакетыОбмена.Вставить(ИмяФайлаОбмена("import", Параметры.ИндексФайлаОбмена), КоммерческаяИнформацияXDTO);
				Параметры.ИндексФайлаОбмена = Параметры.ИндексФайлаОбмена + 1;
				
			КонецЦикла;
			
			// Выгрузить Каталог Предложений
			Если Параметры.ВыборкаЦен.Количество() > 0 Тогда
				
				Параметры.ИндексФайлаОбмена = 0;
				Параметры.НомерВВыборке = Параметры.ВыборкаЦен.Количество();
				Пока Параметры.НомерВВыборке > 0 Цикл
					
					КоммерческаяИнформацияXDTO = ПодготовитьДанныеXDTOПакетПредложений(Параметры, ПакетCML);
					ПакетыОбмена.Вставить(ИмяФайлаОбмена("offers", Параметры.ИндексФайлаОбмена), КоммерческаяИнформацияXDTO);
					Параметры.ИндексФайлаОбмена = Параметры.ИндексФайлаОбмена + 1; 
					
				КонецЦикла;
			КонецЕсли;
			
		КонецЦикла;
		
	КонецЕсли;
	
	Если Параметры.ОбменЗаказами Тогда
		
		МассивИзменений = Новый Массив;
		Если ВыгружатьТолькоИзменения
			И НЕ НастройкаИнтеграции.ВыполнятьПолнуюВыгрузкуПринудительно Тогда
			
			МассивИзменений = Параметры.СтруктураИзменений.Заказы;
		КонецЕсли;
		
		Если НЕ ВыгружатьТолькоИзменения 
			ИЛИ МассивИзменений.Количество() > 0 
			ИЛИ НастройкаИнтеграции.ВыполнятьПолнуюВыгрузкуПринудительно Тогда
			
			// Получить данные заказов
			СтруктураСтатистики = НоваяСтруктураСтатистики();
			ПолучитьДанныеЗаказов(МассивИзменений, СтруктураСтатистики, Параметры);
			
			Параметры.ИндексФайлаОбмена = 0;
			Параметры.НомерВВыборке = Параметры.ВыборкаДокументов.Количество();
			
			Пока Параметры.НомерВВыборке > 0 Цикл
				
				ДокументыXDTO = СформироватьЗаказыXDTO(Параметры, СтруктураСтатистики);
				
				Если СтруктураСтатистики.Выгружено.Количество() = 0 Тогда
					Прервать;
				КонецЕсли;
				
				ПакетыОбмена.Вставить(ИмяФайлаОбмена("orders-", Параметры.ИндексФайлаОбмена, Истина), ДокументыXDTO);
				Параметры.ИндексФайлаОбмена = Параметры.ИндексФайлаОбмена + 1;
				
			КонецЦикла;
			
		КонецЕсли; 
		
	КонецЕсли;
	
	ЗаписьНаУслугиУспешноЗагружены = Истина;
	Если Параметры.ОбменЗаписьНаУслуги Тогда
		
		ПараметрыДляУслуг = ПараметрыДляУслуг(Параметры);
		ПараметрыДляУслуг.Вставить("РучнаяНастройкаКаталога", 0);
		ПараметрыДляУслуг.Вставить("ОтборНоменклатураРучнаяНастройкаКаталога", Неопределено);
		
		ПодготовитьДанныеДляВыгрузки(ПараметрыДляУслуг);
		
		// Если нет данных по номенклатуре, не формируем XDTO-объекты.
		Если ПараметрыДляУслуг.ВыборкаНоменклатуры.Количество() > 0 
			И НЕ ПараметрыДляУслуг.ТаблицаГрафикЗанятостиРесурсов.Пустой() Тогда
			
			URIПространстваИмен = "urn:1C.ru:commerceml_210";
			ПакетCML = ФабрикаXDTO.Пакеты.Получить(URIПространстваИмен);
			
			ПараметрыДляУслуг.ИндексФайлаОбмена = 1;
			ПараметрыДляУслуг.НомерВВыборке = ПараметрыДляУслуг.ВыборкаНоменклатуры.Количество();
			Пока ПараметрыДляУслуг.НомерВВыборке > 0 Цикл
				
				КоммерческаяИнформацияXDTO = ПодготовитьДанныеXDTOКлассификаторИКаталог(ПараметрыДляУслуг, ПакетCML);
				
				ПакетыОбмена.Вставить(ИмяФайлаОбмена("services", ПараметрыДляУслуг.ИндексФайлаОбмена), КоммерческаяИнформацияXDTO);
				ПараметрыДляУслуг.ИндексФайлаОбмена = ПараметрыДляУслуг.ИндексФайлаОбмена + 1;
			КонецЦикла;
			
			Если ПараметрыДляУслуг.ВыборкаЦен.Количество() > 0
				ИЛИ НЕ ПараметрыДляУслуг.ТаблицаГрафикЗанятостиРесурсов.Пустой() Тогда
				
				ПараметрыДляУслуг.ВыполнятьОбменЦены = Истина;
				
				ПараметрыДляУслуг.ИндексФайлаОбмена = 0;
				ПараметрыДляУслуг.НомерВВыборке = ПараметрыДляУслуг.ВыборкаЦен.Количество();
				Если ПараметрыДляУслуг.ВыборкаЦен.Количество() = 0 И НЕ ПараметрыДляУслуг.ТаблицаГрафикЗанятостиРесурсов.Пустой() Тогда
					// если для услуг цены не заданы, выгрузим только график ресурсов
					КоммерческаяИнформацияXDTO = ПодготовитьДанныеXDTOПакетПредложений(ПараметрыДляУслуг, ПакетCML);
					
					ТекстФайла=СериализоватьОбъект(КоммерческаяИнформацияXDTO);
					ПакетыОбмена.Вставить(ИмяФайлаОбмена("services_offers", ПараметрыДляУслуг.ИндексФайлаОбмена), ТекстФайла);
					ПараметрыДляУслуг.ИндексФайлаОбмена = ПараметрыДляУслуг.ИндексФайлаОбмена + 1;
				Иначе
					Пока ПараметрыДляУслуг.НомерВВыборке > 0 Цикл
						
						КоммерческаяИнформацияXDTO = ПодготовитьДанныеXDTOПакетПредложений(ПараметрыДляУслуг, ПакетCML);
						
						ПакетыОбмена.Вставить(ИмяФайлаОбмена("services_offers", ПараметрыДляУслуг.ИндексФайлаОбмена), КоммерческаяИнформацияXDTO);
						ПараметрыДляУслуг.ИндексФайлаОбмена = ПараметрыДляУслуг.ИндексФайлаОбмена + 1;
					КонецЦикла;
				КонецЕсли; 
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЕсли;
	
	// Запись файлов во временный каталог и помещение в хранилище
	ФайлыОбменаДляОтправки = Новый Массив;
	
	Если Параметры.Свойство("СтрокаТаблицыКаталогов") Тогда
		СтруктураРезультата = Параметры.СтрокаТаблицыКаталогов.СтруктураРезультата;
	Иначе
		СтруктураРезультата = Новый Структура("ВыгруженоТоваров,ВыгруженоКартинок,ВыгруженоФайлов,ВыгруженоПредложений,ОписаниеОшибки", 0, 0, 0, 0, "");
	КонецЕсли;
	
	Для Каждого КлючЗначение Из ПакетыОбмена Цикл
		РезультатЗаписи = ЗаписатьФайлXML(КлючЗначение.Значение, СтруктураРезультата.ОписаниеОшибки, ПараметрыКонтекст);
		Если РезультатЗаписи <> Неопределено Тогда
			ОписаниеПередаваемогоФайла = Новый ОписаниеПередаваемогоФайла;
			ОписаниеПередаваемогоФайла.Имя = КлючЗначение.Ключ;
			ОписаниеПередаваемогоФайла.Хранение = РезультатЗаписи;
			ФайлыОбменаДляОтправки.Добавить(ОписаниеПередаваемогоФайла);
		КонецЕсли;
	КонецЦикла;
	
	Возврат ФайлыОбменаДляОтправки;
	
КонецФункции

// Получает заказы покупателей, которые были модифицированы.
//
// Параметры:
//	ДатаИзменения - дата-время, начиная с которого заказ был изменен.
//
// Возвращаемое значение:
//	Массив заказов покупателей.
//
Функция ПолучитьЗаказыДляВыгрузки(ДатаИзменения)
	
	Если ДатаИзменения = '00010101' Тогда
		Возврат Новый Массив;
	КонецЕсли;
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	ЗаказыПокупателейССайта.ЗаказПокупателя КАК ЗаказПокупателя
	|ИЗ
	|	РегистрСведений.ЗаказыПокупателейССайта КАК ЗаказыПокупателейССайта
	|ГДЕ
	|	ЗаказыПокупателейССайта.ЗаказПокупателя.ДатаИзменения >= &ДатаИзменения";
	
	Запрос.УстановитьПараметр("ДатаИзменения", ДатаИзменения);
	
	Возврат Запрос.Выполнить().Выгрузить().ВыгрузитьКолонку("ЗаказПокупателя");
	
КонецФункции // ПолучитьЗаказыДляВыгрузки()

Функция ЗагрузитьЗаписьНаУслугиHTTPСервис(ДанныеЗаказов, КодНастройкиИнтеграции) Экспорт
	
	Если КодНастройкиИнтеграции = Неопределено Тогда
		Возврат Новый Структура("УспешноЗагружено, ОписаниеОшибки, ОтветJSON", Ложь, "Не передан параметр 'SettingsExchangeCode' - код настройки в 1С", "");
	КонецЕсли; 
	
	НастройкаИнтеграции = Справочники.НастройкиИнтеграцииСИнтернетМагазином.ПолучитьСсылку(Новый УникальныйИдентификатор(КодНастройкиИнтеграции));
	Если НЕ ЗначениеЗаполнено(НастройкаИнтеграции) Тогда
		Возврат Новый Структура("УспешноЗагружено, ОписаниеОшибки, ОтветJSON", Ложь, "Не найдена настройка по коду " + КодНастройкиИнтеграции, "");
	КонецЕсли; 
	
	УспешноЗагружено = Ложь;	
	Параметры = ПолучитьСтруктуруОсновныхПараметровОбмена();
	ДобавитьНастройкиВПараметры(НастройкаИнтеграции, Параметры);
	
	СтруктураСтатистики = НоваяСтруктураСтатистики();
	Параметры.Вставить("ОписаниеОшибки", "");
	Параметры.Вставить("ОбменЧерезВебСервис", Истина);
	Параметры.Вставить("ВыгружатьТолькоИзменения", Истина);
	
	ТаблицаИнформации = НоваяТаблицаИнформации();
	СтрокаТаблицыИнформации = ТаблицаИнформации.Добавить();
	ДатаНачала = ТекущаяДатаСеанса();
	СтрокаТаблицыИнформации.ДатаНачала = ДатаНачала;
	
	УспешноЗагружено = ЗагрузитьЗаписьНаУслуги(ДанныеЗаказов, СтруктураСтатистики, Параметры);
	
	ЗаписатьИнформациюПоЗаказамВТаблицуИнформации(ТаблицаИнформации,
	ДатаНачала,
	Перечисления.ДействияПриОбмене.ЗагрузкаДанных,
	УспешноЗагружено,
	СтруктураСтатистики,
	Параметры.ОписаниеОшибки,
	Параметры
	);
	
	ОтветJSON = СтруктураВСтрокуJSON(Новый Структура("УспешноЗагружено, ОписаниеОшибки, СтруктураСтатистики", УспешноЗагружено, Параметры.ОписаниеОшибки, СтруктураСтатистики));
	Возврат Новый Структура("УспешноЗагружено, ОписаниеОшибки, ОтветJSON", УспешноЗагружено, Параметры.ОписаниеОшибки, ОтветJSON);
	
КонецФункции

Функция ЗагрузитьЗаписьНаУслуги(ДанныеЗаказов, СтруктураСтатистики, Параметры) Экспорт
	
	ОписаниеОшибки = Параметры.ОписаниеОшибки;
	
	УчетнаяЗапись = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( Параметры, "НастройкаИнтеграции", Неопределено );
	
	Если ТипЗнч(ДанныеЗаказов) = Тип("ОбъектXDTO") Тогда
		ЗаказыXDTO = ДанныеЗаказов;
	Иначе
		ЗаказыXDTO = ПолучитьЗаказыXDTO(ДанныеЗаказов, Параметры, ОписаниеОшибки);
	КонецЕсли;
	
	Если ЗаказыXDTO = Неопределено Тогда 
		
		Возврат Ложь;
		
	ИначеЕсли НЕ ОбъектXDTOСодержитСвойство(ЗаказыXDTO, "Документ") Тогда
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки, НСтр("ru = 'Нет документов для обработки.'"));
		Возврат Истина;
		
	КонецЕсли;
	
	МассивНомеров = Новый Массив;
	
	МассивЗаказовXDTO = ПолучитьМассивОбъектовСпискаXDTO(ЗаказыXDTO.Документ);
	Для Каждого ДокументXDTO Из МассивЗаказовXDTO Цикл 
		МассивНомеров.Добавить(ДокументXDTO.Номер);
	КонецЦикла;
	
	РанееЗагруженныеСобытия = ПолучитьРанееЗагруженныеДокументы(МассивНомеров);
	
	НачатьТранзакцию();
	
	Попытка
		
		Успешно = СоздатьОбновитьЗаказНаряды(МассивЗаказовXDTO, РанееЗагруженныеСобытия, СтруктураСтатистики, Параметры);
		
		ЗафиксироватьТранзакцию();
		
	Исключение
		
		ТекстОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ОтменитьТранзакцию();
		
		Успешно = Ложь;
		
		ЗаписьЖурналаРегистрации(НСтр("ru = 'Загрузка записей на услуги с сайта'", ОбщегоНазначения.КодОсновногоЯзыка()),
		УровеньЖурналаРегистрации.Ошибка,
		Параметры.НастройкаИнтеграции.Метаданные(),
		Параметры.НастройкаИнтеграции,
		ТекстОшибки);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, УчетнаяЗапись, Истина );
	КонецПопытки;
	
	Если Не Успешно Тогда
		Возврат Ложь;
	КонецЕсли;

	
	// Устанавливаем статус отмены для отмененных заказов.
	
	МассивОтмененныхЗаказов = ПолучитьОтмененныеЗаказы(СтруктураСтатистики.Загружено);
	Если МассивОтмененныхЗаказов.Количество() > 0 Тогда
		
		Для Каждого ДокументСсылка Из МассивОтмененныхЗаказов Цикл
			
			ДокументОбъект = ДокументСсылка.ПолучитьОбъект();
			ДокументОбъект.СостояниеЗаказа = Справочники.СостоянияЗаказовПокупателей.Завершен;
			ДокументОбъект.ВариантЗавершения = Перечисления.ВариантыЗавершенияЗаказа.Отменен;
			
			Попытка
				
				ЗаписатьДокумент(Параметры, ДокументОбъект);
				
			Исключение
				
				ТекстОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
				
				ДобавитьОписаниеОшибки(ОписаниеОшибки,
				ОписаниеИсключительнойОшибки(ТекстОшибки, НСтр("ru = 'Не удалось отменить заказ: '")
				+ ДокументСсылка));
				
				ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, УчетнаяЗапись, Истина );
			КонецПопытки;
			
		КонецЦикла;
		
	КонецЕсли;
	
	// Заказы
	ПроведенныеЗаказы = Новый Массив;
	Для Каждого СсылкаНаДокумент Из СтруктураСтатистики.Загружено Цикл
		
		РежимЗаписи = РежимЗаписиДокумента.Проведение;
		
		ДокументОбъект = СсылкаНаДокумент.ПолучитьОбъект();
		Если ДокументОбъект.ПометкаУдаления
			ИЛИ (ДокументОбъект.ВидЗаказа = Перечисления.ВидыОперацийЗаказПокупателя.ЗаказНаПродажу И НЕ ЗначениеЗаполнено(ДокументОбъект.ДатаОтгрузки)) Тогда
			
			РежимЗаписи = РежимЗаписиДокумента.Запись;
		КонецЕсли;
		
		ДокументОбъект.ОбменДанными.Загрузка = Ложь;
		
		Попытка
			
			ДокументОбъект.Записать(РежимЗаписи);
			СсылкаНаДокумент = ДокументОбъект.Ссылка;
			ПроведенныеЗаказы.Добавить(СсылкаНаДокумент);
			
		Исключение
			ТекстОшибки = ПодробноеПредставлениеОшибки( ИнформацияОбОшибке() );
			ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ТекстОшибки, УчетнаяЗапись, Истина );
		КонецПопытки;
		
		Если Не Параметры.ОбменЧерезВебСервис Тогда
			УдалитьРегистрациюИзмененийУзлаОбмена(Параметры.УзелОбменаЗаказы, СсылкаНаДокумент);
		КонецЕсли;
		
	КонецЦикла;
	
	АссистентУправления.ВыполнитьТекущиеЗадачиСейчас(ПроведенныеЗаказы);
	
	Возврат Истина;
	
КонецФункции

#КонецОбласти

