		// Пустой запрос
		ТекстЗапроса = ТекстЗапроса + Символы.ПС + ";" + Символы.ПС +
		"УНИЧТОЖИТЬ ВременнаяТаблицаНоменклатура";
	КонецЕсли;
	
	// Единицы измерения в Классификатор
	ТекстЗапроса = ТекстЗапроса + Символы.ПС + ";" + Символы.ПС +
	"ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	""41"" КАК НомерЗапроса,
	|	ВременнаяТаблицаЦены.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
