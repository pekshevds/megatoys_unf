// Формирует объект ИнтернетПрокси по настройкам прокси.
//
// Параметры:
//	Прокси		- Соответствие -
//					ключи:
//					НеИспользоватьПроксиДляЛокальныхАдресов - строка - 
//					Сервер			- адрес прокси-сервера
//					Порт			- порт прокси-сервера
//					Пользователь	- имя пользователя для авторизации на прокси-сервере
//					Пароль			- пароль пользователя
//	Протокол	- строка - протокол для которого устанавливаются параметры прокси сервера.
//					Например: "http", "https", "ftp".
//
Функция СформироватьПроксиНовый(НастройкиПрокси, Протокол)
	
	Прокси = Новый ИнтернетПрокси;
	
	НастройкиПроксиПорт = СтроковыеФункцииКлиентСервер.СтрокаВЧисло(НастройкиПрокси["Порт"]);
	
	Прокси.НеИспользоватьПроксиДляЛокальныхАдресов = НастройкиПрокси["НеИспользоватьПроксиДляЛокальныхАдресов"];
	Прокси.Установить(Протокол, НастройкиПрокси["Сервер"], НастройкиПроксиПорт);
	Прокси.Пользователь = НастройкиПрокси["Пользователь"];
	Прокси.Пароль = НастройкиПрокси["Пароль"];
	
	Возврат Прокси;
	
КонецФункции

// Формирует структуру параметров подключения к сайту по настройке обмена
// с учетом параметров прокси.
//
// Параметры:
//	НастройкаИнтеграции - СправочникСсылка.НастройкиИнтеграции
//	НастройкиПодключения - Структура настроек подключения к сайту
//	ОписаниеОшибки - Строка
//
// Возвращаемое значение:
//	Булево - Истина, если успех.
//
Процедура ЗаполнитьНастройкиПодключения(НастройкиПодключения)
	
	СтруктураURI = ОбщегоНазначенияКлиентСервер.СтруктураURI(НастройкиПодключения.АдресСайта);
	
	НастройкиПодключения.Порт = СтруктураURI.Порт;
	НастройкиПодключения.Сервер = СтруктураURI.Хост;
	НастройкиПодключения.Протокол = СтруктураURI.Схема;
	НастройкиПодключения.АдресСкрипта = СтруктураURI.ПутьНаСервере;
	НастройкиПодключения.ЗащищенноеСоединение = (НастройкиПодключения.Протокол = "https");
	
	Если НастройкиПодключения.АдресСкрипта = "" Тогда
		
		Если НастройкиПодключения.ТипСайта = ПредопределенноеЗначение("Перечисление.ПротоколыОбменаCMS.Битрикс") Тогда
			НастройкиПодключения.АдресСкрипта = "/bitrix/admin/1c_exchange.php";
		ИначеЕсли НастройкиПодключения.ТипСайта = ПредопределенноеЗначение("Перечисление.ПротоколыОбменаCMS.UMI") Тогда
			НастройкиПодключения.АдресСкрипта = "/admin/exchange/autoimport/";
		Иначе
			НастройкиПодключения.АдресСкрипта = "bitrix/admin/1c_exchange.php";
		КонецЕсли;
		
	КонецЕсли;
	
	НастройкаПроксиСервера = ПолучениеФайловИзИнтернета.НастройкиПроксиНаСервере();
	
	Если НастройкаПроксиСервера <> Неопределено
		И НастройкаПроксиСервера["ИспользоватьПрокси"] = Истина Тогда
		
		НастройкиПодключения.Вставить("Прокси", СформироватьПроксиНовый(НастройкаПроксиСервера, НастройкиПодключения.Протокол));

	КонецЕсли;
	
КонецПроцедуры

Функция ВыполнитьАвторизациюДляСоединенияНовый(НастройкиПодключения, ТипСоединения = "catalog", ОписаниеОшибки = "", Параметры = Неопределено )
	
	ОписаниеОшибки = "";
	
	РезультатАвторизации = Новый Структура;
	РезультатАвторизации.Вставить("Результат", Ложь);
	РезультатАвторизации.Вставить("Текст", "");
	РезультатАвторизации.Вставить("Соединение", Неопределено);
	
	РезультатАвторизации.Соединение = УстановитьСоединениеССерверомНовый(НастройкиПодключения, ОписаниеОшибки);
	
	Если РезультатАвторизации.Соединение = Неопределено Тогда
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки, НСтр("ru = 'Авторизация не выполнена.'"));
		РезультатАвторизации.Текст = ОписаниеОшибки;
		Возврат РезультатАвторизации;
		
	КонецЕсли;
	
	АдресСкрипта = СтрШаблон("%1?type=%2&mode=checkauth", НастройкиПодключения.АдресСкрипта, ТипСоединения);
	
	ОтветСервера = ПолучитьДанныеССервера(
	РезультатАвторизации.Соединение, 
	АдресСкрипта, 
	"", 
	ОписаниеОшибки,
	Параметры
	);
	
	Если ОтветСервера = Неопределено Тогда 
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки, НСтр("ru = 'Авторизация не выполнена.'"));
		РезультатАвторизации.Текст = ОписаниеОшибки;
		Возврат РезультатАвторизации;
		
	КонецЕсли;
	
	Если СокрЛП(НРег(СтрПолучитьСтроку(ОтветСервера, 1))) <> "success" Тогда
		
		// Иногда ответ сервера - это текст, который содержит пояснение ошибки, тогда его можно вывести пользователю
		// Если ответ сервера - это html страница, тогда не выводим
		Если СтрДлина(ОтветСервера) < 255 И Лев(ОтветСервера, 15) <> "<!DOCTYPE html>" И СтрНайти(Лев(ОтветСервера,255), "<html")=0 Тогда
			ДобавитьОписаниеОшибки(ОписаниеОшибки, ОтветСервера);
		КонецЕсли;
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки, НСтр("ru = 'Авторизация не выполнена.'")
		+ Символы.ПС 
		+ НСтр("ru = 'Не удалось установить соединение с сервером. Проверьте адрес сайта, имя пользователя и пароль.'")
		);
		
		РезультатАвторизации.Текст = ОписаниеОшибки;
		Возврат РезультатАвторизации;
		
	КонецЕсли;
	
	РезультатАвторизации.Результат = Истина;
	РезультатАвторизации.Текст = ОтветСервера;
	
	Возврат РезультатАвторизации;
	
КонецФункции

Функция УстановитьСоединениеССерверомНовый(НастройкиПодключения, ОписаниеОшибки)
	
	Соединение = Неопределено;
	
	Попытка
		
		ЗащищенноеСоединение = Неопределено;
		
		Если НастройкиПодключения.ЗащищенноеСоединение = Истина Тогда
			ЗащищенноеСоединение = ОбщегоНазначенияКлиентСервер.НовоеЗащищенноеСоединение();
		КонецЕсли;
		
		Соединение = Новый HTTPСоединение(
		НастройкиПодключения.Сервер,
		НастройкиПодключения.Порт,
		НастройкиПодключения.Логин,
		НастройкиПодключения.Пароль,
		НастройкиПодключения.Прокси,
		НастройкиПодключения.Таймаут,
		ЗащищенноеСоединение
		);
		
	Исключение
		
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки,
		ОписаниеИсключительнойОшибки(ПодробноеПредставлениеОшибки,
		СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
		НСтр("ru = 'Не удалось установить соединение с сервером %1:%2. Проверьте правильность адреса сервера, порт, имя пользователя и пароль.'"),
		НастройкиПодключения.Сервер,
		НастройкиПодключения.Порт))
		);
		
		НастройкаИнтеграции = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( НастройкиПодключения, "НастройкаИнтеграции", Неопределено );
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ПодробноеПредставлениеОшибки, НастройкаИнтеграции, Истина );
		
		Соединение = Неопределено;
		
	КонецПопытки;
	
	Возврат Соединение;
	
КонецФункции

