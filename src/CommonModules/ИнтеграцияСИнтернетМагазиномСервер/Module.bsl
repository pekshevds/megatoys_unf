Функция ЭтоГруппаВерхнегоУровня(Элемент, СписокГрупп)
	
	Попытка
		
		Если ТипЗнч(СписокГрупп) = Тип("СписокЗначений") Тогда 
			Для Каждого Группа Из СписокГрупп Цикл
				Если НЕ ЗначениеЗаполнено(Группа.Значение) Тогда
					Продолжить;
				ИначеЕсли Группа.Значение.ПринадлежитЭлементу(Элемент) Тогда
					Возврат Истина;
				КонецЕсли; 
			КонецЦикла;
		КонецЕсли;
		
	Исключение
		
		Возврат Ложь;
		
	КонецПопытки;
	
	Возврат Ложь;
	
КонецФункции

Функция ПолучитьВариантыЗначенийСвойстваПоТипу(Выборка, Тип)
	
	ВариантыЗначений = Новый Массив;
	ВыборкаЗначений = Выборка.Выбрать();
	
	Пока ВыборкаЗначений.Следующий() Цикл
		
		Если ТипЗнч(ВыборкаЗначений.Значение) = Тип Тогда
			
			ВариантыЗначений.Добавить(ВыборкаЗначений.Значение);
			
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат ВариантыЗначений;
	
КонецФункции

Функция ПолучитьСтрокуЗаписиДляXML(Значение)
	
	СтрокаЗаписи = Строка(Значение);
	
	Если ТипЗнч(Значение) = Тип("Число") Тогда
		
		СтрокаЗаписи = СтрЗаменить(СтрокаЗаписи, Символы.НПП, "");
		СтрокаЗаписи = СтрЗаменить(СтрокаЗаписи, ",", ".");
		
	ИначеЕсли ТипЗнч(Значение) = Тип("Булево") Тогда
		
		Если Значение Тогда
			СтрокаЗаписи = "true";
		Иначе
			СтрокаЗаписи = "false";
		КонецЕсли;
		
	КонецЕсли;
	
	СтрокаЗаписи = СокрЛП(СтрокаЗаписи);
	
	Возврат СтрокаЗаписи;
	
КонецФункции

// Удаляет все содержимое каталога.
// При ошибке возвращает описание ошибки.
//
// Параметры:
//	Каталог - Строка, путь к каталогу
//	ОписаниеОшибки - Строка, описание возникшей ошибки
//
// Возвращаемое значение:
//	Булево - Истина, если успех, Ложь - если произошла ошибка.
//
Функция ОчиститьКаталог(Каталог, ОписаниеОшибки)
	
	Попытка
		
		УдалитьФайлы(Каталог, "*.*");
		
	Исключение
		
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		
		ДобавитьОписаниеОшибки(ОписаниеОшибки,
		ОписаниеИсключительнойОшибки(ПодробноеПредставлениеОшибки, НСтр("ru = 'Не удалось очистить каталог обмена, п.3: ")
		+ " (" + Каталог + ")"));
		
		Возврат Ложь;
		
	КонецПопытки;
	
	Возврат Истина;
	
КонецФункции

// Формирует структуру параметров подключения к сайту по URL.
//
// Параметры:
//	АдресСайта - Строка, URL
//	НастройкиПодключения - Структура настроек подключения к сайту
//	ОписаниеОшибки - Строка
//
// Возвращаемое значение:
//	Булево - Истина, если успех.
//
Функция РазобратьАдресСайта(НастройкиПодключения, ОписаниеОшибки)
	
	АдресСайта = СокрЛП(НастройкиПодключения.АдресСайта);
	
	Сервер = ""; 
	
	Порт = 0;
	
	АдресСкрипта = "";
	
	ЗащищенноеСоединение = Ложь;
	
	Если НЕ ПустаяСтрока(АдресСайта) Тогда
		
		АдресСайта = СтрЗаменить(АдресСайта, "\", "/");
		АдресСайта = СтрЗаменить(АдресСайта, " ", "");
		
		Если НРег(Лев(АдресСайта, 7)) = "http://" Тогда
			АдресСайта = Сред(АдресСайта, 8);
		ИначеЕсли НРег(Лев(АдресСайта, 8)) = "https://" Тогда
			АдресСайта = Сред(АдресСайта, 9);
			ЗащищенноеСоединение = Истина;
		КонецЕсли;
		
		ПозицияСлэша = СтрНайти(АдресСайта, "/");
		
		Если ПозицияСлэша > 0 Тогда
			Сервер = Лев(АдресСайта, ПозицияСлэша - 1);
			АдресСкрипта = Прав(АдресСайта, СтрДлина(АдресСайта) - ПозицияСлэша);
		Иначе	
			Сервер = АдресСайта;
			АдресСкрипта = "";
		КонецЕсли;
		
		ПозицияДвоеточия = СтрНайти(Сервер, ":");
		ПортСтрока = "0";
		Если ПозицияДвоеточия > 0 Тогда
			СерверСПортом = Сервер;
			Сервер = Лев(СерверСПортом, ПозицияДвоеточия - 1);
			ПортСтрока = Прав(СерверСПортом, СтрДлина(СерверСПортом) - ПозицияДвоеточия);
		КонецЕсли;
		
		Попытка
			
			Порт = Число(ПортСтрока);
			
		Исключение
			
			ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
			
			ДобавитьОписаниеОшибки(ОписаниеОшибки,
			ОписаниеИсключительнойОшибки(ПодробноеПредставлениеОшибки, НСтр("ru = 'Не удалось получить номер порта: '")
			+ ПортСтрока + Символы.ПС
			+ НСтр("ru = 'Проверьте правильность ввода адреса сайта.'")));
			
			Возврат Ложь;
			
		КонецПопытки;
		
		Если Порт = 0 Тогда
			Порт = ?(ЗащищенноеСоединение, 443, 80);
		КонецЕсли;
		
	КонецЕсли;
	
	ТипСайта = Неопределено;
	НастройкиПодключения.Свойство("ТипСайта" , ТипСайта);
	
	Если АдресСкрипта = "" Тогда
		
		Если ТипСайта = ПредопределенноеЗначение("Перечисление.ПротоколыОбменаCMS.Битрикс") Тогда
			АдресСкрипта = "/bitrix/admin/1c_exchange.php";
		ИначеЕсли ТипСайта = ПредопределенноеЗначение("Перечисление.ПротоколыОбменаCMS.UMI") Тогда
			АдресСкрипта = "/admin/exchange/autoimport/";
		Иначе
			АдресСкрипта = "bitrix/admin/1c_exchange.php";
		КонецЕсли;
		
	КонецЕсли;
	
	НастройкиПодключения.Вставить("Сервер", Сервер); 
	НастройкиПодключения.Вставить("Порт", Порт);
	НастройкиПодключения.Вставить("АдресСкрипта", АдресСкрипта);
	НастройкиПодключения.Вставить("ЗащищенноеСоединение", ЗащищенноеСоединение);
	
	Возврат Истина;
	
КонецФункции

// Выполняет необходимые действия при завершении обмена.
//
// Параметры:
//	Параметры - Структура основных параметров
//	ТаблицаИнформации - Таблица значений, состояние текущего сеанса обмена
//	Ошибка - Булево, Истина, если необходимо зафиксировать завершение обмена с ошибками.
//
Процедура ВыполнитьДействияПриЗавершенииОбмена(Параметры, Ошибка = Ложь)
	
	ТаблицаИнформации = Параметры.ТаблицаИнформации;
	ТаблицаИнформации.ЗаполнитьЗначения(Параметры.НастройкаИнтеграции, "НастройкаИнтеграции");
	
	// Записываем информацию по каждому действию в журнал регистрации.
	
	Для Каждого СтрокаТаблицыИнформации Из ТаблицаИнформации Цикл
		
		СобытиеЖурнала = ОбменДаннымиСервер.КлючСообщенияЖурналаРегистрации(Параметры.НастройкаИнтеграции, СтрокаТаблицыИнформации.ДействиеПриОбмене);
		
		Если СтрокаТаблицыИнформации.РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.Выполнено Тогда
			УровеньЖурнала = УровеньЖурналаРегистрации.Информация;
		Иначе
			УровеньЖурнала = УровеньЖурналаРегистрации.Ошибка;
			ЛогОшибка = Истина;
		КонецЕсли;
		
		Если Ошибка Тогда
			УровеньЖурнала = УровеньЖурналаРегистрации.Ошибка;
			ЛогОшибка = Истина;
		КонецЕсли;
		
		СтрокаОписания = Параметры.РежимЗапускаОбмена + Символы.ПС +
		?(Параметры.ВыгружатьТолькоИзменения, "Только изменения", "Полный обмен") + Символы.ПС;
		
		ЛогКомментарий = СтрокаОписания + СтрокаТаблицыИнформации.Описание;
		
		ЗаписьЖурналаРегистрации(СобытиеЖурнала,
		УровеньЖурнала,
		Параметры.НастройкаИнтеграции.Метаданные(),
		Параметры.НастройкаИнтеграции,
		ЛогКомментарий);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ЛогКомментарий, Параметры.НастройкаИнтеграции, ЛогОшибка );
	КонецЦикла;
	
	// Объединяем 2 строки информации по выгрузке (товары и заказы) в одну (ВыгрузкаДанных).
	
	СтрокиВыгрузки = ТаблицаИнформации.НайтиСтроки(Новый Структура("ДействиеПриОбмене", Перечисления.ДействияПриОбмене.ВыгрузкаДанных));
	
	Если СтрокиВыгрузки.Количество() = 2 Тогда
		
		Если СтрокиВыгрузки[1].РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.Ошибка Тогда
			СтрокиВыгрузки[0].РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.Ошибка;
			СтрокиВыгрузки[0].Описание = СтрокиВыгрузки[1].Описание;
		Иначе
			
			СтрокиВыгрузки[0].Описание = СтрокиВыгрузки[0].Описание
			+ Символы.ПС + Символы.ПС + "=====" + Символы.ПС + Символы.ПС
			+ СтрокиВыгрузки[1].Описание;
		КонецЕсли;
		
		ТаблицаИнформации.Удалить(СтрокиВыгрузки[1]);
		
	КонецЕсли;
	
	// Отметим выгруженные на сайт объекты в регистре соответствия объектов 1С и сайта
	Если Параметры.Свойство("СоответствияОбъектовИнтернетМагазина") И Параметры.ВыгружатьНаСайт = Истина Тогда
		
		МассивОбъектов = Новый Массив;
		Для Каждого структураКлюч Из Параметры.СоответствияОбъектовИнтернетМагазина Цикл
			МассивОбъектов.Добавить(структураКлюч.Ключ);
		КонецЦикла;
		Если МассивОбъектов.Количество()>0 Тогда
			
			Запрос = Новый Запрос;
			Запрос.Текст = 
			"ВЫБРАТЬ
			|	СоответствияОбъектовИнтернетМагазина.КлючИнтеграции КАК КлючИнтеграции,
			|	СоответствияОбъектовИнтернетМагазина.ИдентификаторМагазина КАК ИдентификаторМагазина,
			|	СоответствияОбъектовИнтернетМагазина.ИдентификаторБазы КАК ИдентификаторБазы,
			|	СоответствияОбъектовИнтернетМагазина.СсылкаНаОбъект КАК СсылкаНаОбъект,
			|	ИСТИНА КАК ОбъектВыгружен,
			|	СоответствияОбъектовИнтернетМагазина.ТипОбъекта КАК ТипОбъекта,
			|	СоответствияОбъектовИнтернетМагазина.ДополнительныеПараметры КАК ДополнительныеПараметры
			|ИЗ
			|	РегистрСведений.СоответствияОбъектовИнтернетМагазина КАК СоответствияОбъектовИнтернетМагазина
			|ГДЕ
			|	СоответствияОбъектовИнтернетМагазина.СсылкаНаОбъект В(&МассивОбъектов)
			|	И СоответствияОбъектовИнтернетМагазина.КлючИнтеграции = &КлючИнтеграции
			|	И НЕ СоответствияОбъектовИнтернетМагазина.ОбъектВыгружен";
			
			Запрос.УстановитьПараметр("МассивОбъектов", МассивОбъектов);
			Запрос.УстановитьПараметр("КлючИнтеграции", ПолучитьКлючПоНастройке(Параметры.НастройкаИнтеграции));
			
			Результат = Запрос.Выполнить();
			
			Выборка = Результат.Выбрать();
			Пока Выборка.Следующий() Цикл
				СоответствияОбъектовИнтернетМагазинаМенеджер = РегистрыСведений.СоответствияОбъектовИнтернетМагазина.СоздатьМенеджерЗаписи();
				ЗаполнитьЗначенияСвойств(СоответствияОбъектовИнтернетМагазинаМенеджер, Выборка);
				
				СоответствияОбъектовИнтернетМагазинаМенеджер.Записать(Истина);
			КонецЦикла;
			
		КонецЕсли;
	КонецЕсли; 
	
	// Записываем состояния обмена.
	
	УстановитьПривилегированныйРежим(Истина);

	ВыгруженныеТоварыИнтернетМагазинаСтатистика = ВыгруженныеТоварыИнтернетМагазинаСтатистикаПолучить( Параметры );

	ЕстьОшибка = Ложь;
	МассивОшибок = Новый Массив;
	Для Каждого СтрокаТаблицыИнформации Из ТаблицаИнформации Цикл
		
		ЗаписьСостояния = РегистрыСведений.СостоянияОбменовИнтернетМагазина.СоздатьМенеджерЗаписи();
		ЗаписьСостояния.НастройкаИнтеграции = СтрокаТаблицыИнформации.НастройкаИнтеграции;
		ЗаписьСостояния.ДействиеПриОбмене = СтрокаТаблицыИнформации.ДействиеПриОбмене;
		ЗаписьСостояния.Прочитать();
		
		ЗаполнитьЗначенияСвойств(ЗаписьСостояния, СтрокаТаблицыИнформации, , "ДанныеПоследнегоОбмена, ДанныеПоследнегоНепустогоОбмена, ПоследнийТекстОшибки");
		
		Если СтрокаТаблицыИнформации.ДействиеПриОбмене = Перечисления.ДействияПриОбмене.ВыгрузкаДанных Тогда
			Если СтрокаТаблицыИнформации.РезультатВыполненияОбмена <> Перечисления.РезультатыВыполненияОбмена.Выполнено Тогда
				ЗаписьСостояния.ПоследнийТекстОшибки = СтрокаТаблицыИнформации.Описание;
			КонецЕсли;
		ИначеЕсли СтрокаТаблицыИнформации.ДействиеПриОбмене = Перечисления.ДействияПриОбмене.ЗагрузкаДанных Тогда
			Если Параметры.Свойство("ДанныеПоследнегоОбмена") Тогда
				ЗаписьСостояния.ДанныеПоследнегоОбмена = Новый ХранилищеЗначения(Параметры.ДанныеПоследнегоОбмена, Новый СжатиеДанных(9));
			КонецЕсли;
			Если Параметры.Свойство("ДанныеПоследнегоНепустогоОбмена") Тогда
				ЗаписьСостояния.ДанныеПоследнегоНепустогоОбмена = Новый ХранилищеЗначения(Параметры.ДанныеПоследнегоНепустогоОбмена, Новый СжатиеДанных(9));
			КонецЕсли;
			Если СтрокаТаблицыИнформации.РезультатВыполненияОбмена <> Перечисления.РезультатыВыполненияОбмена.Выполнено Тогда
				ЗаписьСостояния.ПоследнийТекстОшибки = СтрокаТаблицыИнформации.Описание + Символы.ПС + Параметры.СтрокаXMLФайлаЗаказов;
			КонецЕсли;
		КонецЕсли;
		
		// Даты записываем по границам сеанса, чтобы работал отбор журнала.
		ЗаписьСостояния.ДатаНачала = Параметры.ДатаФормирования;
		ЗаписьСостояния.ДатаОкончания = ТекущаяДатаСеанса();
		
		ЗаписьСостояния.Записать();
		
		Если ЗначениеЗаполнено(ЗаписьСостояния.ДействиеПриОбмене)
			И (ЗаписьСостояния.РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.Выполнено
			ИЛИ ЗаписьСостояния.РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.ВыполненоСПредупреждениями) Тогда
			
			ЗаписьУспешногоСостояния = РегистрыСведений.СостоянияУспешныхОбменовИнтернетМагазина.СоздатьМенеджерЗаписи();
			ЗаполнитьЗначенияСвойств(ЗаписьУспешногоСостояния, ЗаписьСостояния);
			
			Если ЗаписьСостояния.ДействиеПриОбмене = Перечисления.ДействияПриОбмене.ВыгрузкаДанных Тогда
				ЗаписьУспешногоСостоянияДанные = Новый Структура;
				ЗаписьУспешногоСостоянияДанные.Вставить( "ВыгруженныеТоварыИнтернетМагазинаСтатистика", ВыгруженныеТоварыИнтернетМагазинаСтатистика );
				ЗаписьУспешногоСостояния.Данные = Новый ХранилищеЗначения( ЗаписьУспешногоСостоянияДанные, Новый СжатиеДанных(9) );
			ИначеЕсли ЗаписьСостояния.ДействиеПриОбмене = Перечисления.ДействияПриОбмене.ЗагрузкаДанных Тогда
				ЗаказыСтатистика = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( Параметры, "ЗаказыСтатистика", Неопределено );
				Если ЗаказыСтатистика <> Неопределено Тогда
					ЗаписьУспешногоСостоянияДанные = Новый Структура;
					ЗаписьУспешногоСостоянияДанные.Вставить( "ЗаказыСтатистика", ЗаказыСтатистика );
					ЗаписьУспешногоСостояния.Данные = Новый ХранилищеЗначения( ЗаписьУспешногоСостоянияДанные, Новый СжатиеДанных(9) );
				КонецЕсли;
			КонецЕсли;
			
			ЗаписьУспешногоСостояния.Записать();
			
		КонецЕсли;
		
		Если СтрокаТаблицыИнформации.РезультатВыполненияОбмена = Перечисления.РезультатыВыполненияОбмена.Ошибка Тогда
			
			ЕстьОшибка = Истина;
			Если Не ПустаяСтрока( ЗаписьСостояния.ПоследнийТекстОшибки ) Тогда
				МассивОшибок.Добавить( ЗаписьСостояния.ПоследнийТекстОшибки );
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЦикла;
	
	Если ЕстьОшибка Тогда
		
		ТекстОшибки = СтрСоединить(МассивОшибок, ";" + Символы.ПС);
		
		ОбщегоНазначения.СообщитьПользователю( ТекстОшибки );
		
		СоздатьОтправитьЭлектронноеПисьмо(Параметры.НастройкаИнтеграции,
		НСтр("ru = 'Интеграция с интернет-магазином. Ошибка обмена.'") + " ( " + Параметры.НастройкаИнтеграции + " )",
		ТекстОшибки
		);
		
		Если Параметры.ЭтоРеглЗадание Тогда
			ДобавитьОшибкуВСчетчикРегламентногоЗаданияИОтключитьПриПревышенииСУведомлением(Параметры.НастройкаИнтеграции, Параметры.ДатаФормирования, ТекстОшибки);
		КонецЕсли;
	Иначе
		Если Параметры.ЭтоРеглЗадание Тогда
			// задание обмена выполнено без ошибок, сбросим счётчик ошибок
			СчетчикОшибокРегламентногоЗаданияОчистить( Параметры.НастройкаИнтеграции );
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

Функция ВыгруженныеТоварыИнтернетМагазинаСтатистикаПолучить( Параметры )
	
	Результат = Новый Соответствие;
	
	ТаблицаКаталогов = Параметры.СохраненнаяТаблицаКаталогов.Получить();
	
	ВыгруженныеТоварыИнтернетМагазина = Параметры.ВыгруженныеТоварыИнтернетМагазина.Скопировать();
	ВыгруженныеТоварыИнтернетМагазина.Колонки.Добавить( "Количество" );
	ВыгруженныеТоварыИнтернетМагазина.ЗаполнитьЗначения( 1, "Количество" );
	ВыгруженныеТоварыИнтернетМагазина.Свернуть( "ИдентификаторКаталога, Действие", "Количество" );
	ДействиеТоварДобавить = Перечисления.ОбменСВнешнимиСистемамиЛогированиеДействия.ТоварДобавить;
	Если ВыгруженныеТоварыИнтернетМагазина.Количество() = 0 Тогда
	
		Для Каждого ТаблицаКаталоговСтрока Из ТаблицаКаталогов Цикл
		
			КаталогНаименование = ТаблицаКаталоговСтрока.Каталог;
			
			КаталогЭлемент = Новый Соответствие;
			КаталогЭлемент.Вставить( "КаталогНаименование", КаталогНаименование );
			Результат.Вставить( ТаблицаКаталоговСтрока.ИдентификаторКаталога, КаталогЭлемент );
			
		КонецЦикла;
		
	Иначе
	
		Для Каждого ВыгруженныеТоварыИнтернетМагазинаСтрока Из ВыгруженныеТоварыИнтернетМагазина Цикл
			
			КаталогЭлемент = Результат.Получить( ВыгруженныеТоварыИнтернетМагазинаСтрока.ИдентификаторКаталога );
			Если КаталогЭлемент = Неопределено Тогда
				Результат.Вставить( ВыгруженныеТоварыИнтернетМагазинаСтрока.ИдентификаторКаталога, Новый Соответствие );
				КаталогЭлемент = Результат.Получить( ВыгруженныеТоварыИнтернетМагазинаСтрока.ИдентификаторКаталога );
				
				ТаблицаКаталоговСтрока = ТаблицаКаталогов.Найти( ВыгруженныеТоварыИнтернетМагазинаСтрока.ИдентификаторКаталога, "ИдентификаторКаталога" );
				Если ТаблицаКаталоговСтрока <> Неопределено Тогда
					КаталогНаименование = СокрЛП( ТаблицаКаталоговСтрока.Каталог );
				Иначе
					КаталогНаименование = "<Удаленный каталог>";
				КонецЕсли;
				КаталогЭлемент.Вставить( "КаталогНаименование", КаталогНаименование );
			КонецЕсли;
			
			КаталогЭлемент.Вставить( ВыгруженныеТоварыИнтернетМагазинаСтрока.Действие, ВыгруженныеТоварыИнтернетМагазинаСтрока.Количество );
			
		КонецЦикла;
		
	КонецЕсли;
	
	Возврат Результат;
КонецФункции

Процедура ЗаписатьДанныеОВыгрузкеТоваров(НастройкаИнтеграции, Параметры)
	
	Если Параметры.ВыгружатьТолькоИзменения И НЕ Параметры.ВыполнятьПолнуюВыгрузкуПринудительно Тогда
		
		Для Каждого СтрОписание Из Параметры.ВыгруженныеОстаткиИнтернетМагазина Цикл
			МЗ = РегистрыСведений.ВыгруженныеОстаткиИнтернетМагазина.СоздатьМенеджерЗаписи();
			ЗаполнитьЗначенияСвойств(МЗ, СтрОписание);
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
			МЗ.Записать();
		КонецЦикла;
		
		Для Каждого СтрОписание Из Параметры.ВыгруженныеЦеныИнтернетМагазина Цикл
			МЗ = РегистрыСведений.ВыгруженныеЦеныИнтернетМагазина.СоздатьМенеджерЗаписи();
			ЗаполнитьЗначенияСвойств(МЗ, СтрОписание);
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
			МЗ.Записать();
		КонецЦикла;

	Иначе
		
		НЗ = РегистрыСведений.ВыгруженныеОстаткиИнтернетМагазина.СоздатьНаборЗаписей();
		НЗ.Отбор.НастройкаИнтеграцииСИнтернетМагазином.Установить(НастройкаИнтеграции);
		НЗ.Записать();
		Для Каждого СтрОписание Из Параметры.ВыгруженныеОстаткиИнтернетМагазина Цикл
			МЗ = РегистрыСведений.ВыгруженныеОстаткиИнтернетМагазина.СоздатьМенеджерЗаписи();
			ЗаполнитьЗначенияСвойств(МЗ, СтрОписание);
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
			МЗ.Записать();
		КонецЦикла;
		
		НЗ = РегистрыСведений.ВыгруженныеЦеныИнтернетМагазина.СоздатьНаборЗаписей();
		НЗ.Отбор.НастройкаИнтеграцииСИнтернетМагазином.Установить(НастройкаИнтеграции);
		НЗ.Записать();
		Для Каждого СтрОписание Из Параметры.ВыгруженныеЦеныИнтернетМагазина Цикл
			МЗ = РегистрыСведений.ВыгруженныеЦеныИнтернетМагазина.СоздатьМенеджерЗаписи();
			ЗаполнитьЗначенияСвойств(МЗ, СтрОписание);
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
			МЗ.Записать();
		КонецЦикла;
		
	КонецЕсли;

	ВыгруженныеТоварыИнтернетМагазинаЗаписать( НастройкаИнтеграции, Параметры );
	
КонецПроцедуры

Процедура ВыгруженныеТоварыИнтернетМагазинаЗаписать( НастройкаИнтеграции, Параметры )
	
	Если Параметры.ОбменТоварами = Ложь Тогда
		Возврат;
	КонецЕсли;
	
	Если Параметры.ВыгружатьНаСайт = Ложь Тогда
		Возврат;
	КонецЕсли;
	
	ТоварДействиеДобавление = Перечисления.ОбменСВнешнимиСистемамиЛогированиеДействия.ТоварДобавить;
	ТоварДействиеИзменение = Перечисления.ОбменСВнешнимиСистемамиЛогированиеДействия.ТоварИзменить;
	ТоварДействиеУдаление = Перечисления.ОбменСВнешнимиСистемамиЛогированиеДействия.ТоварУдалить;
	
	ТоварыОтбор = Новый Структура( "ИдентификаторКаталога, Номенклатура, Характеристика" );

	Для Каждого Элемент Из Параметры.ВыгруженныеТоварыИнтернетМагазина Цикл
		
		ТоварДействиеНовое = Неопределено;

		ТоварыОтбор.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
		ТоварыОтбор.Номенклатура = Элемент.Номенклатура;
		ТоварыОтбор.Характеристика = Элемент.Характеристика;
		ВыгруженныеТоварыИнтернетМагазинаПередНачаломВыгрузкиСтроки = Параметры.ВыгруженныеТоварыИнтернетМагазинаПередНачаломВыгрузки.НайтиСтроки( ТоварыОтбор );
		
		Если ВыгруженныеТоварыИнтернетМагазинаПередНачаломВыгрузкиСтроки.Количество() = 0 Тогда
			
			Если Элемент.Действие = ТоварДействиеУдаление Тогда
				ТоварДействиеНовое = ТоварДействиеУдаление;
			Иначе 
				ТоварДействиеНовое = ТоварДействиеДобавление;
			КонецЕсли;
			
		Иначе
			
			ТоварДействиеТекущее = ВыгруженныеТоварыИнтернетМагазинаПередНачаломВыгрузкиСтроки[ 0 ].Действие;
			
			Если ТоварДействиеТекущее = ТоварДействиеУдаление Тогда
				
				Если Элемент.Действие = ТоварДействиеУдаление Тогда
					ТоварДействиеНовое = ТоварДействиеУдаление;
				Иначе
					ТоварДействиеНовое = ТоварДействиеДобавление;
				КонецЕсли;
				
			ИначеЕсли ТоварДействиеТекущее = ТоварДействиеДобавление Тогда
				
				Если Элемент.Действие = ТоварДействиеУдаление Тогда
					ТоварДействиеНовое = ТоварДействиеУдаление;
				Иначе
					ТоварДействиеНовое = ТоварДействиеИзменение;
				КонецЕсли;
				
			Иначе
				
				ТоварДействиеНовое = ТоварДействиеИзменение;
				
			КонецЕсли;
			
		КонецЕсли;

		Элемент.Действие = ТоварДействиеНовое;
		
		МЗ = РегистрыСведений.ВыгруженныеТоварыИнтернетМагазина.СоздатьМенеджерЗаписи();
		
		МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
		МЗ.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
		МЗ.Номенклатура = Элемент.Номенклатура;
		МЗ.Характеристика = Элемент.Характеристика;
		
		Если ТоварДействиеНовое <> ТоварДействиеИзменение Тогда
			МЗ.Прочитать();
			Если МЗ.Выбран() И МЗ.Действие = ТоварДействиеНовое Тогда
				Продолжить;
			КонецЕсли;
		КонецЕсли;
		
		МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
		МЗ.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
		МЗ.Номенклатура = Элемент.Номенклатура;
		МЗ.Характеристика = Элемент.Характеристика;
		МЗ.Действие = ТоварДействиеНовое;
		МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
		
		МЗ.Записать();
		
		УчетнаяЗапись = НастройкаИнтеграции;
		
		Если ТоварДействиеНовое <> ТоварДействиеИзменение Тогда
			Если ОбменСВнешнимиСистемамиЛогированиеПовтИсп.ЛогированиеВключено( УчетнаяЗапись ) Тогда
				ЛогКомментарий = "Каталог: " + Элемент.ИдентификаторКаталога;
				
				ДанныеСсылка = ?( ЗначениеЗаполнено( Элемент.Характеристика ), Элемент.Характеристика, Элемент.Номенклатура );
				ЛогПараметры = Новый Структура;
				ЛогПараметры.Вставить( "Действие", ТоварДействиеНовое );
				ЛогПараметры.Вставить( "ДанныеСсылка", ДанныеСсылка );
				ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ЛогКомментарий, УчетнаяЗапись, , ЛогПараметры );
			Конецесли;
		Конецесли;
		
	КонецЦикла;
	
    Если Параметры.ВыгружатьТолькоИзменения = Ложь Тогда
    	// при полном обмене у старых записей поменять статус на Удаление
    	Для Каждого Элемент Из Параметры.ВыгруженныеТоварыИнтернетМагазинаПередНачаломВыгрузки Цикл

			ТоварыОтбор.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
			ТоварыОтбор.Номенклатура = Элемент.Номенклатура;
			ТоварыОтбор.Характеристика = Элемент.Характеристика;
			ВыгруженныеТоварыИнтернетМагазинаСтроки = Параметры.ВыгруженныеТоварыИнтернетМагазина.НайтиСтроки( ТоварыОтбор );
		
			Если ВыгруженныеТоварыИнтернетМагазинаСтроки.Количество() > 0 Тогда
				Продолжить;
			Конецесли;

			Если Элемент.Действие <> Перечисления.ОбменСВнешнимиСистемамиЛогированиеДействия.ТоварУдалить Тогда
				ВыгруженныеТоварыИнтернетМагазинаСтрока = Параметры.ВыгруженныеТоварыИнтернетМагазина.Добавить();
				ЗаполнитьЗначенияСвойств( ВыгруженныеТоварыИнтернетМагазинаСтрока, Элемент );
				ВыгруженныеТоварыИнтернетМагазинаСтрока.Действие = ТоварДействиеУдаление;
			КонецЕсли;
			
			ТоварДействиеНовое = ТоварДействиеУдаление;
			
			МЗ = РегистрыСведений.ВыгруженныеТоварыИнтернетМагазина.СоздатьМенеджерЗаписи();
			
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
			МЗ.Номенклатура = Элемент.Номенклатура;
			МЗ.Характеристика = Элемент.Характеристика;
			
			Если ТоварДействиеНовое <> ТоварДействиеИзменение Тогда
				МЗ.Прочитать();
				Если МЗ.Выбран() И МЗ.Действие = ТоварДействиеНовое Тогда
					Продолжить;
				КонецЕсли;
			КонецЕсли;
			
			МЗ.НастройкаИнтеграцииСИнтернетМагазином = НастройкаИнтеграции;
			МЗ.ИдентификаторКаталога = Элемент.ИдентификаторКаталога;
			МЗ.Номенклатура = Элемент.Номенклатура;
			МЗ.Характеристика = Элемент.Характеристика;
			МЗ.Действие = ТоварДействиеНовое;
			МЗ.ДатаВыгрузки = Параметры.ДатаФормирования;
			
			МЗ.Записать();
			
			УчетнаяЗапись = НастройкаИнтеграции;
			Если ТоварДействиеНовое <> ТоварДействиеИзменение Тогда
				Если ОбменСВнешнимиСистемамиЛогированиеПовтИсп.ЛогированиеВключено( УчетнаяЗапись ) Тогда
					ЛогКомментарий = "Каталог: " + Элемент.ИдентификаторКаталога;
					
					ДанныеСсылка = ?( ЗначениеЗаполнено( Элемент.Характеристика ), Элемент.Характеристика, Элемент.Номенклатура );
					ЛогПараметры = Новый Структура;
					ЛогПараметры.Вставить( "Действие", ТоварДействиеНовое );
					ЛогПараметры.Вставить( "ДанныеСсылка", ДанныеСсылка );
					ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ЛогКомментарий, УчетнаяЗапись, , ЛогПараметры );
				Конецесли;
			Конецесли;
    		
    	КонецЦикла;
    	
    КонецЕсли;
    
КонецПроцедуры

Процедура УдалитьРегистрациюИзменений(НастройкаИнтеграции, Параметры, ОбменТоварами, ОбменЗаказами, ОбменУслугами = Ложь)
	
	НомерСообщенияДляРегистрацииИзменений = НомерСообщенияДляРегистрацииИзмененийПолучить();
	
	Если ОбменТоварами Тогда
		Если ЗначениеЗаполнено( Параметры.УзелОбменаТовары ) Тогда
			ПланыОбмена.УдалитьРегистрациюИзменений( Параметры.УзелОбменаТовары, НомерСообщенияДляРегистрацииИзменений );
		КонецЕсли;
	КонецЕсли;
	
	Если ОбменЗаказами Тогда
		Если ЗначениеЗаполнено( Параметры.УзелОбменаЗаказы ) Тогда
			ПланыОбмена.УдалитьРегистрациюИзменений( Параметры.УзелОбменаЗаказы, НомерСообщенияДляРегистрацииИзменений );
		КонецЕсли;
	КонецЕсли;
	
	Если ОбменУслугами Тогда
		Если ЗначениеЗаполнено( Параметры.УзелОбменаУслуги ) Тогда
			ПланыОбмена.УдалитьРегистрациюИзменений( Параметры.УзелОбменаУслуги, НомерСообщенияДляРегистрацииИзменений );
		КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

Процедура УдалитьРегистрациюИзмененийУзлаОбмена(УзелОбмена, Данные)
	Попытка
		ПланыОбмена.УдалитьРегистрациюИзменений(УзелОбмена, Данные);
	Исключение
		
		Комментарий = "Ошибка: УдалитьРегистрациюИзмененийУзлаОбмена: " + Символы.ПС
		+ Данные + Символы.ПС
		+ ЗначениеВСтрокуВнутр(Данные)
		+ Символы.ПС + ПодробноеПредставлениеОшибки( ИнформацияОбОшибке() );
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",УровеньЖурналаРегистрации.Ошибка,
		Метаданные.Справочники.НастройкиИнтеграцииСИнтернетМагазином, Данные, Комментарий);
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( Комментарий, , Истина );
		
	КонецПопытки;
КонецПроцедуры

Функция ПолучитьРеквизитыЗаказаНаСайте(ЗаказПокупателя)
	
	Запрос = Новый Запрос("ВЫБРАТЬ
	|	ЗаказыПокупателейССайта.НомерЗаказаНаСайте КАК НомерЗаказаНаСайте,
	|	ЗаказыПокупателейССайта.ДатаЗаказаНаСайте КАК ДатаЗаказаНаСайте
	|ИЗ
	|	РегистрСведений.ЗаказыПокупателейССайта КАК ЗаказыПокупателейССайта
	|ГДЕ
	|	ЗаказыПокупателейССайта.ЗаказПокупателя = &ЗаказПокупателя");
	
	Запрос.УстановитьПараметр("ЗаказПокупателя", ЗаказПокупателя);
	
	Результат = Запрос.Выполнить();
	Если Результат.Пустой()Тогда
		
		Возврат Неопределено;
		
	КонецЕсли;
	
	Выборка = Результат.Выбрать();
	Выборка.Следующий();
	
	СтруктураВозврата = Новый Структура;
	СтруктураВозврата.Вставить("НомерЗаказаНаСайте", Выборка.НомерЗаказаНаСайте);
	СтруктураВозврата.Вставить("ДатаЗаказаНаСайте", Выборка.ДатаЗаказаНаСайте);
	
	Возврат СтруктураВозврата;
	
КонецФункции

