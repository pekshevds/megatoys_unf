
#Область ПрограммныйИнтерфейс

Функция ВыполнитьТестовоеПодключениеКСайту(НастройкаИнтеграции) Экспорт
	
	РезультатПодключения = ВыполнитьПодключениеКСайту(НастройкаИнтеграции);
	РезультатПодключения.Вставить("Соединение", Неопределено);
	
	Возврат РезультатПодключения;
	
КонецФункции

Функция ВыполнитьПодключениеКСайту(НастройкаИнтеграции) Экспорт
	
	НастройкиПодключения = ПолучитьНастройкиПодключенияНовый(НастройкаИнтеграции);
	НастройкиПодключения.Вставить( "НастройкаИнтеграции", НастройкаИнтеграции );
	
	РезультатПодключения = Новый Структура;
	РезультатПодключения.Вставить("Результат", Ложь);
	РезультатПодключения.Вставить("Текст", "");
	РезультатПодключения.Вставить("Соединение", Неопределено);
	
	ОписаниеОшибки = "";

	ПараметрыКонтекст = Новый Структура;
	ПараметрыКонтекст.Вставить( "НастройкаИнтеграции", НастройкаИнтеграции );
	
	РезультатАвторизации = ВыполнитьАвторизациюДляСоединенияНовый(НастройкиПодключения, "catalog", ОписаниеОшибки, ПараметрыКонтекст);
	Если РезультатАвторизации.Результат Тогда
		
		РезультатПодключения.Результат = Истина;
		РезультатПодключения.Соединение = РезультатАвторизации.Соединение;
		РезультатПодключения.Текст = НСтр("ru = 'Соединение с сайтом успешно установлено.'");
		
	Иначе
		
		РезультатПодключения.Текст = НСтр("ru = 'Не удалось установить соединение.'") + Символы.ПС + РезультатАвторизации.Текст;
		
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ОписаниеОшибки) Тогда
		
		ЗаписьЖурналаРегистрации("ИнтеграцияСИнтернетМагазином",
		УровеньЖурналаРегистрации.Ошибка,
		НастройкаИнтеграции.Метаданные(),
		НастройкаИнтеграции,
		ОписаниеОшибки);
		
	КонецЕсли;
	
	
	Возврат РезультатПодключения;
	
КонецФункции

Функция ЗагрузитьНастройкиБитрикс(НастройкаИнтеграции) Экспорт
	
	НастройкиПодключения = ПолучитьНастройкиПодключенияНовый(НастройкаИнтеграции);
	
	ОписаниеОшибки = "";

	ПараметрыКонтекст = Новый Структура;
	ПараметрыКонтекст.Вставить( "НастройкаИнтеграции", НастройкаИнтеграции );
	
	РезультатАвторизации = ВыполнитьАвторизациюДляСоединенияНовый(НастройкиПодключения, "sale", , ПараметрыКонтекст );
	Если НЕ РезультатАвторизации.Результат Тогда
		Возврат "";
	КонецЕсли;

	ОтветСервера = РезультатАвторизации.Текст;
	
	КукиИмя = СтрПолучитьСтроку(ОтветСервера, 2);
	КукиЗначение = СтрПолучитьСтроку(ОтветСервера, 3);
	ЗаголовкиЗапросов = "Cookie: " + КукиИмя + "=" + КукиЗначение;
	
	Попытка
		Токен = "&" + СтрПолучитьСтроку(ОтветСервера, 4);
	Исключение
		Возврат "";
	КонецПопытки;
	
	СтрокаЗапроса = "bitrix/admin/1c_exchange.php?type=sale&mode=info"+Токен;
	
	ОтветСервера = ПолучитьДанныеССервера(
	РезультатАвторизации.Соединение,
	СтрокаЗапроса,
	ЗаголовкиЗапросов,
	ОписаниеОшибки,
	ПараметрыКонтекст
	);
	
	Если ОтветСервера = Неопределено Тогда 
		ДобавитьОписаниеОшибки(ОписаниеОшибки, НСтр("ru = 'Загрузка видов оплат не выполнена.'"));
		Возврат "";
	КонецЕсли;
	
	СтрокаXML = "";
	
	Если Лев(ОтветСервера, 2) = "PK" Тогда
		СтрокаXML = РаспаковатьZIPАрхив(ОтветСервера, ОписаниеОшибки);
	Иначе
		Если Лев(ОтветСервера, 5) = "<?xml" Тогда
			СтрокаXML = ОтветСервера;
		КонецЕсли;
	КонецЕсли;
	
	Возврат СтрокаXML;
	
КонецФункции

Функция ЗагрузитьНастройкиUMI(НастройкаИнтеграции) Экспорт
	
