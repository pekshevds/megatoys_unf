Функция ИнтернетМагазинДанныеДляВыгрузкиПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст = Неопределено ) Экспорт

	Если ТипЗнч( ПараметрыКонтекст ) <> Тип( "Структура" ) Тогда
		ПараметрыКонтекст = Новый Структура;
	КонецЕсли;
	ПараметрыКонтекст.Вставить( "ОтчетПоДаннымДляВыгрузки", Истина );
	
	Если НЕ ЗначениеЗаполнено(НастройкаИнтернетМагазина) Тогда
		Возврат Ложь;
	КонецЕсли;
	
	Параметры = ИнтеграцияСИнтернетМагазиномСервер.ПолучитьСтруктуруОсновныхПараметровОбмена();
	
	Параметры.Вставить("ЭтоРеглЗадание", Ложь);
	Параметры.Вставить("ОбменЧерезВебСервис", Ложь);
	Параметры.Вставить("РежимЗапускаОбмена", "Получение данных для отчета");
	Параметры.Вставить("ВыгружатьТолькоИзменения", Ложь );
	Параметры.Вставить("СоответствиеФайлов", Новый Соответствие);
	
	СистемнаяИнформация = Новый СистемнаяИнформация;
	ПлатформаWindows = СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86
	ИЛИ СистемнаяИнформация.ТипПлатформы = ТипПлатформы.Windows_x86_64;
	
	Параметры.Вставить("ПлатформаWindows", ПлатформаWindows);
	
	ДобавитьНастройкиВПараметры(НастройкаИнтернетМагазина, Параметры);
	
	Параметры.Вставить("ТаблицаИнформации", НоваяТаблицаИнформации());
	Параметры.Вставить("ВыгружатьНаСайт", Ложь);
	
	Параметры.Вставить("ОтчетПоДаннымДляВыгрузки", Истина);

	ПолучитьСтруктуруИзменений( Параметры );

	НастройкиИнтеграцииПараметрыДляПодмены = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "НастройкиИнтеграцииПараметрыДляПодмены", Неопределено );
	Если ТипЗнч( НастройкиИнтеграцииПараметрыДляПодмены ) = Тип( "Структура" ) Тогда
		Для Каждого НастройкиИнтеграцииПараметрыДляПодменыЭлемент Из НастройкиИнтеграцииПараметрыДляПодмены Цикл
			Параметры.Вставить(НастройкиИнтеграцииПараметрыДляПодменыЭлемент.Ключ, НастройкиИнтеграцииПараметрыДляПодменыЭлемент.Значение);
		КонецЦикла;
	КонецЕсли;
	
	ДиагностикаТовараОтборНоменклатура = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "ДиагностикаТовараОтборНоменклатура", Неопределено );
	Если ЗначениеЗаполнено( ДиагностикаТовараОтборНоменклатура ) Тогда
		Параметры.Вставить( "ДиагностикаТовараОтборНоменклатура", ДиагностикаТовараОтборНоменклатура );
	КонецЕсли;
	
	ТоварыУспешноВыгружены = ВыгрузитьНоменклатуру(Параметры);
	ПараметрыКонтекст.Вставить( "ОтчетПоДаннымДляВыгрузкиДанные", Параметры.ОтчетПоДаннымДляВыгрузкиДанные );
	
	Возврат ТоварыУспешноВыгружены;
КонецФункции

Функция ИнтернетМагазинОтчетПоДаннымДляВыгрузкиТабличныйДокументПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст = Неопределено ) Экспорт

	ДанныеДляОтчета = ПараметрыКонтекст.ОтчетПоДаннымДляВыгрузкиДанные;
	
	Если ТипЗнч( ДанныеДляОтчета ) <> Тип( "Массив" ) Тогда
		Возврат Неопределено;
	КонецЕсли;

	Если ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "ОтчетРежимПодробно", Ложь ) = Истина Тогда
		НеВыводитьВОтчет = Новый Массив;
	Иначе
		НеВыводитьВОтчет = "СостояниеНастроек, ПодпискиНаСобытие, ТаблицаИнформации, СоответствиеДополнительныхПолейНоменклатуры,"
		+ "ТаблицаСоответствияСтатусовЗаказов, ВидыЦен, ТаблицаСоответствияВидовОплат, ТаблицаСоответствияСлужбДоставки,"
		+ "СоответствиеПолейНоменклатуры, СтруктураКаталога, СтруктураКаталога, ВыборкаСкладов,"
		+ "ВыборкаВидовЦен, ВыборкаСвойствНоменклатурыДляКлассификатора, ВыборкаСвойствХарактеристикДляКлассификатора,"
		+ "ВыборкаСвойствНоменклатуры, ДанныеОрганизацииВладельцаКаталога, КатегорииНоменклатуры, ВыборкаЕдиницИзмеренияДляКлассификатора,"
		+ "ДопРеквизитыНоменклатуры, ТаблицаОстаткиНаборов, ТаблицаЦеныНаборов, СтруктурныеЕдиницы, ВыборкаКартинкиХарактеристик, ВыборкаФайлов,"
		+ "ВыгруженныеТоварыИнтернетМагазина";
		НеВыводитьВОтчет = СтрРазделить( НеВыводитьВОтчет, " ,", Ложь );
	КонецЕсли;
	
	Отчет = Новый ТабличныйДокумент;
	
	ОтчетСтрокаНомер = 0;
	
	ОтчетСтрокаНомер = Отчет.ВысотаТаблицы + 1;
	ТекущаяОбласть = Отчет.Область( ОтчетСтрокаНомер, 1 );
	ТекущаяОбласть.Текст = "Интернет-магазин: " + НастройкаИнтернетМагазина;
	ТекущаяОбласть.Шрифт = ШрифтыСтиля.ВажнаяНадписьШрифт;
	ТекущаяОбласть.ШиринаКолонки = 100;
	
	Для Каждого КаталогДанныеДляОтчета Из ДанныеДляОтчета Цикл
		
		ОтчетСтрокаНомер = Отчет.ВысотаТаблицы + 2;
		ТекущаяОбласть = Отчет.Область( ОтчетСтрокаНомер, 1, ОтчетСтрокаНомер, 1 );
		ТекущаяОбласть.Текст = "Каталог: " + КаталогДанныеДляОтчета.КаталогНаименование + ": " + КаталогДанныеДляОтчета.КаталогИдентификатор;
		ТекущаяОбласть.Шрифт = ШрифтыСтиля.ВажнаяНадписьШрифт;
		
		ТекущаяОбласть = Отчет.Область( ОтчетСтрокаНомер, 1, ОтчетСтрокаНомер, 5 );
		ТекущаяОбласть.ЦветФона = ЦветаСтиля.ВыделеннаяКнопкаФон;
		
		Отчет.НачатьГруппуСтрок();
		
		Для Каждого КаталогДанныеДляОтчетаЭлемент Из КаталогДанныеДляОтчета Цикл
			Ключ = КаталогДанныеДляОтчетаЭлемент.Ключ;
			Если НеВыводитьВОтчет.Найти( Ключ ) <> Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			Значение = КаталогДанныеДляОтчетаЭлемент.Значение;
			
			Если ТипЗнч( Значение ) <> Тип( "ТаблицаЗначений" ) Тогда
				Продолжить;
			КонецЕсли;
			
			ТаблицаЗначений = Значение;
			
			ОтчетСтрокаНомер = Отчет.ВысотаТаблицы + 2;
			ТекущаяОбласть = Отчет.Область( ОтчетСтрокаНомер, 1 );
			ТекущаяОбласть.Текст = "" + Ключ + " ( " + ТаблицаЗначений.Количество() + " )";
			ТекущаяОбласть.Шрифт = ШрифтыСтиля.ВажнаяНадписьШрифт;

			
			Отчет.НачатьГруппуСтрок();
		
			КолонкиСортировки = "";
			КолонкаИмя = "Номенклатура";
			Если ТаблицаЗначений.Колонки.Найти( КолонкаИмя ) <> Неопределено Тогда
				КолонкиСортировки = КолонкиСортировки + ?(ПустаяСтрока( КолонкиСортировки ), "" , "," ) + КолонкаИмя;
			КонецЕсли;
			КолонкаИмя = "Характеристика";
			Если ТаблицаЗначений.Колонки.Найти( КолонкаИмя ) <> Неопределено Тогда
				КолонкиСортировки = КолонкиСортировки + ?(ПустаяСтрока( КолонкиСортировки ), "" , "," ) + КолонкаИмя;
			КонецЕсли;
			Если Не ПустаяСТрока( КолонкиСортировки ) Тогда
				ТаблицаЗначений.Сортировать( КолонкиСортировки );
			КонецЕсли;

			Если ТаблицаЗначений.Колонки.Найти( "НомерСтроки" ) = Неопределено Тогда
				ТаблицаЗначений.Колонки.Вставить( 0, "НомерСтроки" );
				НомерСтроки = 0;
				Для Каждого ТаблицаЗначенийСтрока Из ТаблицаЗначений Цикл
					НомерСтроки = НомерСтроки + 1;
					ТаблицаЗначенийСтрока.НомерСтроки = НомерСтроки;
				КонецЦикла;
			КонецЕсли;
			
			ТаблицаЗначенийВТабличныйДокумент( ТаблицаЗначений, Отчет );
			
			Отчет.ЗакончитьГруппуСтрок();
			
		КонецЦикла;
		Отчет.ЗакончитьГруппуСтрок();
	КонецЦикла;
	
	ТекущаяОбласть = Отчет.Область( 1, 1, Отчет.ВысотаТаблицы, Отчет.ШиринаТаблицы );
	ТекущаяОбласть.СоздатьФорматСтрок();
	Для КолонкаНомер = 1 По Отчет.ШиринаТаблицы Цикл
		ТекущаяОбласть = Отчет.Область( 1, КолонкаНомер, 1, КолонкаНомер );
		ТекущаяОбласть.ШиринаКолонки = 30;
	КонецЦикла;
	
	Отчет.ПоказатьУровеньГруппировокСтрок( 1 );
	Отчет.ПоказатьУровеньГруппировокСтрок( 0 );
	Отчет.ТолькоПросмотр = Истина;
	Отчет.ОтображатьЗаголовки = Ложь;
	Отчет.ОтображатьСетку = Ложь;
	Отчет.ОтображатьГруппировки = Истина;

	Возврат Отчет;
КонецФункции

Функция ИнтернетМагазинОтчетПоДаннымДляВыгрузкиПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст = Неопределено ) Экспорт
	
	Если ТипЗнч( ПараметрыКонтекст ) <> Тип( "Структура" ) Тогда
		ПараметрыКонтекст = Новый Структура;
	КонецЕсли;
	
	Успех = ИнтеграцияСИнтернетМагазиномСервер.ИнтернетМагазинДанныеДляВыгрузкиПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст );
	
	Если Успех <> Истина Тогда
		Возврат Неопределено;
	КонецЕсли;
	
	ОтчетТабличныйДокумент = Неопределено;
	ФормироватьПечатнуюФорму = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "ОтчетФормироватьПечатнуюФорму", Истина );
	Если ФормироватьПечатнуюФорму = Истина Тогда
		ОтчетТабличныйДокумент = ИнтернетМагазинОтчетПоДаннымДляВыгрузкиТабличныйДокументПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст );
	КонецЕсли;
	
	Возврат ОтчетТабличныйДокумент;
КонецФункции

Функция ТаблицаЗначенийВТабличныйДокумент( ТаблицаЗначений, ТабличныйДокумент = Неопределено ) Экспорт
	
	Если ТабличныйДокумент = Неопределено Тогда
		ТабличныйДокумент = Новый ТабличныйДокумент;
	КонецЕсли;
	
	Построитель = Новый ПостроительОтчета;
	Построитель.ИсточникДанных = Новый ОписаниеИсточникаДанных( ТаблицаЗначений );
	Построитель.Вывести( ТабличныйДокумент );
	
	Возврат ТабличныйДокумент;
КонецФункции

Процедура ПередЗаписьюЗаказаПокупателя(Источник, Отказ, РежимЗаписи, РежимПроведения) Экспорт

	Если ТипЗнч( Источник ) <> Тип( "ДокументОбъект.ЗаказПокупателя" ) Тогда
		Возврат;
	КонецЕсли;
	
	Если ОбменСВнешнимиСистемамиЛогированиеПовтИсп.ЛогированиеВключено() <> Истина Тогда
		Возврат;
	КонецЕсли;
	
	Если Источник.Ссылка.Пустая() Тогда
		Возврат;
	КонецЕсли;
	
	ЗаказПокупателяОбъект = Источник;
	ЗаказПокупателяСсылка = Источник.Ссылка;
	
	// запишем изменение значений реквизитов заказа
	ИмяРеквизита = "СостояниеЗаказа";
	ЗначениеДоИзменения = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( ЗаказПокупателяСсылка, ИмяРеквизита );
	ЗначениеПослеИзменения = ЗаказПокупателяОбъект[ ИмяРеквизита ];
	
	Если ЗначениеДоИзменения <> ЗначениеПослеИзменения Тогда
		
		СтекВызоваСтр = ОбменСВнешнимиСистемамиЛогирование.СтекВызоваПолучить();
		ЛогПараметры = Новый Структура( "ДанныеСсылка", ЗаказПокупателяСсылка );

		ЛогКомментарий = "ПередЗаписьюЗаказаПокупателя: Изменено состояние заказа покупателя №"
		+ ЗаказПокупателяОбъект.Номер + " от " + ЗаказПокупателяОбъект.Дата + ": "
		+ ЗначениеДоИзменения + " -> " + ЗначениеПослеИзменения
		+ ОбменСВнешнимиСистемамиЛогирование.КомментарийРазделительБлоков()
		+ СтекВызоваСтр;
		
		ОбменСВнешнимиСистемамиЛогирование.ЗаписатьВЛог( ЛогКомментарий, , , ЛогПараметры );
		
	КонецЕсли;
	
КонецПроцедуры

Функция ДиагностикаТовараВыполнить( ПараметрыКонтекст ) Экспорт
    
    Результат = Неопределено;
    
    НастройкаИнтернетМагазина = ПараметрыКонтекст.НастройкаИнтернетМагазина;
	СохраненнаяТаблицаКаталоговДляЗаменыОсновная = НастройкаИнтернетМагазина.СохраненнаяТаблицаКаталогов.Получить();
	Если СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Количество() = 0 Тогда
    	Сообщить( "Диагностика товара: Каталоги не заданы в настройка интернет-магазина """ + НастройкаИнтернетМагазина + """" );
		Возврат Результат;
	КонецЕсли;

    // оставим только 1 выбранный каталог из таблицы каталогов для проверки
    Если СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Количество() > 1 Тогда
    
		КаталогиОтбор = Новый Структура( "ИдентификаторКаталога", ПараметрыКонтекст.КаталогИдентификатор );
		СохраненнаяТаблицаКаталоговДляЗаменыОсновная = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать( КаталогиОтбор );
		
		Если СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Количество() <> 1 Тогда
    		Сообщить( "Диагностика товара: Идентификатор каталога задан некорректно." );
			Возврат Результат;
		КонецЕсли;
		
    КонецЕсли;
    
    НастройкаКомпоновкиОсновная = СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].ХранилищеНастроекКомпоновки.Получить();
    
	// итерация 1: без изменений
	
	ПараметрыДляПодмены = Новый Структура;
	СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
	ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения( СохраненнаяТаблицаКаталоговДляЗамены ) );
	ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
	
	Результат = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );

    Если Результат.НоменклатураПометкаУдаления = Истина Тогда
    	Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). Номенклатура помечена на удаление. Необходимо снять пометку удаления." );
    	Возврат Результат;
    КонецЕсли;

    Если Результат.ХарактеристикаПометкаУдаления = Истина Тогда
    	Результат.Рекомендации.Добавить( "Не найден в ""Предложения"" (offers.xml). Характеристика помечена на удаление. Необходимо снять пометку удаления." );
    	Возврат Результат;
    КонецЕсли;

    Если Результат.ХарактеристикаНоменклатураИспользоватьХарактеристики = Ложь Тогда
    	РекомендацияСтр = 
    	"Не найден в ""Предложения"" (offers.xml). Владельцем характеристики является номенклатура, "
    	+ "у номенклатуры отключен флажок ""Использовать характеристики"".";
    	Результат.Рекомендации.Добавить( РекомендацияСтр );
    	Возврат Результат;
    КонецЕсли;

    Если Результат.ХарактеристикаКатегорияИспользоватьХарактеристики = Ложь Тогда
    	РекомендацияСтр = 
    	"Не найден в ""Предложения"" (offers.xml). Владельцем характеристики является категория, "
    	+ "у категории отключен флажок ""Использовать характеристики"".";
    	Результат.Рекомендации.Добавить( РекомендацияСтр );
    	Возврат Результат;
    КонецЕсли;
    
    Если Результат.НоменклатураЕстьОсновноеИзображение = Ложь Тогда
    	Результат.Рекомендации.Добавить( "Не задано основное изображение номенклатуры. Необходимо заполнить в форме элемента номенклатуры." );
    КонецЕсли;
    
    Если Результат.ТоварВходитВКаталог = Истина
    И Результат.ТоварВходитВПредложения = Истина Тогда
    	Возврат Результат;
    КонецЕсли;


    ОтборИспользуется = НастройкиКомпоновкиДанныхОтборИспользуется( НастройкаКомпоновкиОсновная );
    

	// итерация 2: очистим Отбор
	Если ОтборИспользуется = Истина Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
		
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока = СохраненнаяТаблицаКаталоговДляЗамены[ 0 ];
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.ХранилищеНастроекКомпоновки = Новый ХранилищеЗначения( Неопределено );// отбор
		
		ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения( СохраненнаяТаблицаКаталоговДляЗамены ) );
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). Необходимо изменить настройку ""Отбора"" на вкладке ""Товары""." );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;


	// итерация 3: отключим ручную настройку каталога
	Если СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].РучнаяНастройкаКаталога <> 0 Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
		
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока = СохраненнаяТаблицаКаталоговДляЗамены[ 0 ];
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.РучнаяНастройкаКаталога = 0;
		
		ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения (СохраненнаяТаблицаКаталоговДляЗамены ) );
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). Необходимо изменить настройку ""Структуры каталога"" на вкладке ""Товары""." );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;


	// итерация 4: очистим группы каталога
	КаталогГруппы = СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].Группы;
	Если КаталогГруппы.Количество() > 0 Тогда
		Если КаталогГруппы.Количество() = 1 
		И Не ЗначениеЗаполнено( КаталогГруппы[ 0 ].Значение )
		Тогда
			// не нужна итерация, список "пустой"
		Иначе
			
			ПараметрыДляПодмены = Новый Структура;
			СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
			
			СохраненнаяТаблицаКаталоговДляЗаменыСтрока = СохраненнаяТаблицаКаталоговДляЗамены[ 0 ];
			СохраненнаяТаблицаКаталоговДляЗаменыСтрока.Группы = Новый СписокЗначений;
			
			ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения (СохраненнаяТаблицаКаталоговДляЗамены ) );
			ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
			
			РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
			
			Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
				И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
				Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). Необходимо изменить настройку ""Группы/Категории"" на вкладке ""Товары""." );
				
				Возврат Результат;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;


	// итерация 5: очистим Отбор и Ручную настройку каталога
	Если ОтборИспользуется = Истина 
	И СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].РучнаяНастройкаКаталога <> 0 Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
		
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока = СохраненнаяТаблицаКаталоговДляЗамены[ 0 ];
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.ХранилищеНастроекКомпоновки = Новый ХранилищеЗначения( Неопределено );
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.РучнаяНастройкаКаталога = 0;
		
		ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения (СохраненнаяТаблицаКаталоговДляЗамены ) );
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). Необходимо изменить настройки ""Отбора"" и ""Структуры каталога"" на вкладке ""Товары""." );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;


	// итерация 6: очистим все ограничения каталога
	Если ОтборИспользуется = Истина 
		И СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].РучнаяНастройкаКаталога <> 0 
		И СохраненнаяТаблицаКаталоговДляЗаменыОсновная[ 0 ].Группы.Количество() > 0 
		Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		СохраненнаяТаблицаКаталоговДляЗамены = СохраненнаяТаблицаКаталоговДляЗаменыОсновная.Скопировать();
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока = СохраненнаяТаблицаКаталоговДляЗамены[ 0 ];
		
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.ХранилищеНастроекКомпоновки = Новый ХранилищеЗначения( Неопределено );
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.РучнаяНастройкаКаталога = 0;
		СохраненнаяТаблицаКаталоговДляЗаменыСтрока.Группы = Новый СписокЗначений;
		
		ПараметрыДляПодмены.Вставить( "СохраненнаяТаблицаКаталогов", Новый ХранилищеЗначения (СохраненнаяТаблицаКаталоговДляЗамены ) );
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			Результат.Рекомендации.Добавить( "Не найден в ""Каталоге"" (import.xml). "
			+ "Необходимо изменить настройки ""Отбора"", ""Структуры каталога"" и ""Группы/Категории"" на вкладке ""Товары""." );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;


	// итерация 7: "Выгружать товары только с ненулевой ценой" в Ложь
	Если ПараметрыКонтекст.НастройкаИнтернетМагазина.ВыгружатьТоварыТолькоСНеНулевойЦеной = Истина Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		ПараметрыДляПодмены.Вставить( "ВыгружатьТоварыТолькоСНеНулевойЦеной", Ложь );
		
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			
			СтрРекомендация = 
			"Не найден в ""Каталоге"" (import.xml). Цены не найдены. "
			+ "Необходимо добавить цены выбранных видов для товара. "
			+ "Либо выключить флаг ""Выгружать товары только с ненулевой ценой"" на вкладке ""Товары"".";
			
			Результат.Рекомендации.Добавить( СтрРекомендация );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;

	// итерация 8: "Выгружать только товары с изображениями" в Ложь
	Если ПараметрыКонтекст.НастройкаИнтернетМагазина.ОбязательноеНаличиеФотографий = Истина Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		ПараметрыДляПодмены.Вставить( "ОбязательноеНаличиеФотографий", Ложь );
		
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			
			СтрРекомендация = "Не найден в ""Каталоге"" (import.xml). "
			+ "Необходимо добавить изображенияс типом jpg, png, gif. "
			+ "Либо изменить настройку ""Выгружать только товары с изображениями"" на вкладке ""Товары"".";
			
			Результат.Рекомендации.Добавить( СтрРекомендация );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;

	// итерация 9: "Выгружать товары только с ненулевой ценой" в Ложь и "Выгружать только товары с изображениями" в Ложь
	Если ПараметрыКонтекст.НастройкаИнтернетМагазина.ОбязательноеНаличиеФотографий = Истина Тогда
		
		ПараметрыДляПодмены = Новый Структура;
		ПараметрыДляПодмены.Вставить( "ВыгружатьТоварыТолькоСНеНулевойЦеной", Ложь );
		ПараметрыДляПодмены.Вставить( "ОбязательноеНаличиеФотографий", Ложь );
		
		ПараметрыКонтекст.Вставить( "НастройкиИнтеграцииПараметрыДляПодмены", ПараметрыДляПодмены );
		
		РезультатДополнительнойИтерации = ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст );
		
		Если РезультатДополнительнойИтерации.ТоварВходитВКаталог = Истина
			И РезультатДополнительнойИтерации.ТоварВходитВПредложения = Истина Тогда
			СтрРекомендация = 
			"Не найден в ""Каталоге"" (import.xml). "
			+ "Необходимо добавить цены выбранных видов для товара и изображения с типом jpg, png, gif. "
			+ "Либо выключить флаг ""Выгружать товары только с ненулевой ценой"" и "
			+ """Выгружать только товары с изображениями"" на вкладке ""Товары"".";
			Результат.Рекомендации.Добавить( СтрРекомендация );
			
			Возврат Результат;
		КонецЕсли;
	КонецЕсли;
	
	Возврат Результат;
КонецФункции

Функция ОбменДаннымиДиагностикаТовараИтерацияВыполнить( ПараметрыКонтекст = Неопределено )
	
	Если ТипЗнч( ПараметрыКонтекст ) <> Тип( "Структура" ) Тогда
		ПараметрыКонтекст = Новый Структура;
	КонецЕсли;
	
	Результат = Новый Структура;
	Результат.Вставить( "Рекомендации", Новый Массив );
	
	НастройкаИнтернетМагазина = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "НастройкаИнтернетМагазина", Неопределено );
	Если НастройкаИнтернетМагазина = Неопределено Тогда
		Сообщить( "Диагностика товара: Настройка интернет-магазина не выбрана." );
		Возврат Результат;
	КонецЕсли;
	
	Номенклатура = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "Номенклатура", Неопределено );
    Если Не ЗначениеЗаполнено( Номенклатура ) Тогда
    	Сообщить( "Диагностика товара: Номенклатура не выбрана." );
    	Возврат Результат;
    КонецЕсли;

	Характеристика = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "Характеристика", Неопределено );
	
	КаталогИдентификатор = ИнтеграцияСМаркетплейсамиСервер.СтруктураСвойствоЗначениеПолучить( ПараметрыКонтекст, "КаталогИдентификатор", Неопределено );
    Если Не ЗначениеЗаполнено( КаталогИдентификатор ) Тогда
    	Сообщить( "Диагностика товара: Каталог не выбран." );
    	Возврат Результат;
    КонецЕсли;
    
	Отчет = ИнтеграцияСИнтернетМагазиномСервер.ИнтернетМагазинОтчетПоДаннымДляВыгрузкиПолучить( НастройкаИнтернетМагазина, ПараметрыКонтекст );
	
	Если Не ПараметрыКонтекст.Свойство( "ОтчетПоДаннымДляВыгрузкиДанные" ) Тогда
		Сообщить( "Диагностика товара: Ошибка при получении данных." );
		Возврат Результат;
	КонецЕсли;
	
	ДанныеДляОтчета = ПараметрыКонтекст.ОтчетПоДаннымДляВыгрузкиДанные;
	Если ТипЗнч( ДанныеДляОтчета ) <> Тип( "Массив" ) Тогда
		Возврат Результат;
	КонецЕсли;
	
	// проверка наличия основного изображения
	НоменклатураОсновноеИзображение = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( Номенклатура, "ФайлКартинки" );
	Результат.Вставить( "НоменклатураОсновноеИзображениеСсылка", НоменклатураОсновноеИзображение );
	Результат.Вставить( "НоменклатураЕстьОсновноеИзображение", ЗначениеЗаполнено( НоменклатураОсновноеИзображение ) );
	
	Если ЗначениеЗаполнено( Характеристика ) Тогда
		ХарактеристикаОсновноеИзображение = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( Характеристика, "ФайлКартинки" );
		Результат.Вставить( "ХарактеристикаОсновноеИзображениеСсылка", ХарактеристикаОсновноеИзображение);
		Результат.Вставить( "ХарактеристикаЕстьОсновноеИзображение", ЗначениеЗаполнено( ХарактеристикаОсновноеИзображение ) );
	Иначе
		Результат.Вставить( "ХарактеристикаОсновноеИзображениеСсылка", Неопределено );
		Результат.Вставить( "ХарактеристикаЕстьОсновноеИзображение", Неопределено );
	КонецЕсли;
	
	// проверка пометки удаления
	НоменклатураПометкаУдаления = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( Номенклатура, "ПометкаУдаления" );
	Результат.Вставить( "НоменклатураПометкаУдаления", НоменклатураПометкаУдаления );
	Если ЗначениеЗаполнено( Характеристика ) Тогда
		ХарактеристикаПометкаУдаления = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( Характеристика, "ПометкаУдаления" );
		Результат.Вставить( "ХарактеристикаПометкаУдаления", ХарактеристикаПометкаУдаления );
	Иначе
		Результат.Вставить( "ХарактеристикаПометкаУдаления", Неопределено );
	КонецЕсли;

	// проверка "Использовать характеристики" у категории, если владелец харакетристики категория
	Результат.Вставить( "ХарактеристикаКатегорияИспользоватьХарактеристики", Неопределено );
	Результат.Вставить( "ХарактеристикаНоменклатураИспользоватьХарактеристики", Неопределено );
	Результат.Вставить( "ХарактеристикаВладелец", Неопределено );
	Если ЗначениеЗаполнено( Характеристика ) Тогда
		ХарактеристикаВладелец = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( Характеристика, "Владелец" );
		Результат.Вставить( "ХарактеристикаВладелец", ХарактеристикаВладелец );

		ИспользоватьХарактеристики = ОбщегоНазначения.ЗначениеРеквизитаОбъекта( ХарактеристикаВладелец, "ИспользоватьХарактеристики" );

		Если ТипЗнч( ХарактеристикаВладелец )  = Тип( "СправочникСсылка.КатегорииНоменклатуры" ) Тогда
			Результат.Вставить( "ХарактеристикаКатегорияИспользоватьХарактеристики", ИспользоватьХарактеристики );
		Иначе
			Результат.Вставить( "ХарактеристикаНоменклатураИспользоватьХарактеристики", ИспользоватьХарактеристики );
		КонецЕсли;
		
	КонецЕсли;
	
	Для Каждого ДанныеДляОтчетаЭлемент Из ДанныеДляОтчета Цикл
	
		Если ДанныеДляОтчетаЭлемент.КаталогИдентификатор <> КаталогИдентификатор Тогда
			Продолжить;
		КонецЕсли;
		
		Результат.Вставить( "КаталогИдентификатор", ДанныеДляОтчетаЭлемент.КаталогИдентификатор );
		Результат.Вставить( "КаталогНаименование", ДанныеДляОтчетаЭлемент.КаталогНаименование );
		
		// Каталог
		ТоварВходитВКаталог = ДиагностикаТоварВходитВКаталог( ДанныеДляОтчетаЭлемент, Номенклатура );
		Результат.Вставить( "ТоварВходитВКаталог", ТоварВходитВКаталог );

		// Изображения
		ФайлыИИзображения = ДиагностикаФайлыПолучить( ДанныеДляОтчетаЭлемент, Номенклатура , Характеристика );
		Результат.Вставить( "Файлы", ФайлыИИзображения.Файлы );
		Результат.Вставить( "ЕстьИзображенияНоменклатуры", ФайлыИИзображения.ЕстьИзображенияНоменклатуры );
		Результат.Вставить( "ЕстьИзображенияХарактеристики", ФайлыИИзображения.ЕстьИзображенияХарактеристики );
		
		// Предложения
		ТоварВходитВПредложения = ДиагностикаТоварВходитВПредложения( ДанныеДляОтчетаЭлемент, Номенклатура , Характеристика );
		Результат.Вставить( "ТоварВходитВПредложения", ТоварВходитВПредложения );

		// Цены
		Цены = ДиагностикаЦеныПолучить( ДанныеДляОтчетаЭлемент, Номенклатура , Характеристика );
		Результат.Вставить( "Цены", Цены );
		
		ЦенаМинимальная = Неопределено;
		ЦенаМаксимальная = Неопределено;
		Если Цены.Количество() > 0 Тогда
			Для Каждого ЦеныСтрока Из Цены Цикл
				Если ЦенаМинимальная = Неопределено Тогда
					ЦенаМинимальная = ЦеныСтрока.Цена;
				Иначе
					ЦенаМинимальная = Мин( ЦенаМинимальная, ЦеныСтрока.Цена );
				КонецЕсли;
				Если ЦенаМаксимальная = Неопределено Тогда
					ЦенаМаксимальная = ЦеныСтрока.Цена;
				Иначе
					ЦенаМаксимальная = Макс( ЦенаМаксимальная, ЦеныСтрока.Цена );
				КонецЕсли;
			КонецЦикла;
		КонецЕсли;
		Результат.Вставить( "ЦенаМинимальная", ЦенаМинимальная );
		Результат.Вставить( "ЦенаМаксимальная", ЦенаМаксимальная );
		
		// Остатки
		Остатки = ДиагностикаОстаткиПолучить( ДанныеДляОтчетаЭлемент, Номенклатура , Характеристика );
		Остаток = Остатки.Итог( "КоличествоНаСкладе" );
		Результат.Вставить( "Остатки", Остатки );
		Результат.Вставить( "Остаток", Остаток );
		
		
		ВыгруженныеРанееТовары = ДиагностикаВыгруженныеРанееТоварыПолучить( ДанныеДляОтчетаЭлемент, Номенклатура , Характеристика );
		Результат.Вставить( "ВыгруженныеРанееТовары", ВыгруженныеРанееТовары );
		
		Прервать;
	КонецЦикла;

	Возврат Результат;
КонецФункции

Функция ДиагностикаТоварВходитВКаталог( ДанныеДляОтчетаЭлемент, Номенклатура )
	ВыборкаНоменклатурыСтрока = ДанныеДляОтчетаЭлемент.ВыборкаНоменклатуры.Найти( Номенклатура, "Номенклатура" );
	Если ВыборкаНоменклатурыСтрока = Неопределено Тогда
		Возврат Ложь;
	Иначе
		Возврат Истина;
	КонецЕсли;
КонецФункции

Функция ДиагностикаТоварВходитВПредложения( ДанныеДляОтчетаЭлемент, Номенклатура, Характеристика )
	Отбор = Новый Структура( "Номенклатура, Характеристика", Номенклатура, Характеристика );
	ВыборкаЦенСтроки = ДанныеДляОтчетаЭлемент.ВыборкаЦен.НайтиСтроки( Отбор );
	Если ВыборкаЦенСтроки.Количество() = 0 Тогда
		Возврат Ложь;
	Иначе
		Возврат Истина;
	КонецЕсли;
КонецФункции

Функция ДиагностикаЦеныПолучить( ДанныеДляОтчетаЭлемент, Номенклатура, Характеристика )
	Отбор = Новый Структура( "Номенклатура, Характеристика", Номенклатура, Характеристика );
	Цены = ДанныеДляОтчетаЭлемент.ВыборкаЦен.Скопировать( Отбор );
	Цены.Сортировать( "ВидЦен" );
	Возврат Цены;
КонецФункции

