		ТекущийЭлемент = Элементы.Найти("НачисленияСумма");
	ИначеЕсли ОткрыватьОтпускИлиБольничный Тогда
		ОткрытьОтпускИлиБольничный();
	КонецЕсли; 
	
КонецПроцедуры

