	ОтчетКомитенту.Контрагент,
	ОтчетКомитенту.Договор,
	0,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ОтчетКомитенту КАК ОтчетКомитенту
ГДЕ
	&amp;КомиссияДоступна = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	ОтчетКомиссионера.Ссылка,
	ОтчетКомиссионера.Дата,
	ОтчетКомиссионера.Организация,
	ОтчетКомиссионера.Контрагент,
	ОтчетКомиссионера.Договор,
	0,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.ОтчетКомиссионера КАК ОтчетКомиссионера
ГДЕ
	&amp;КомиссияДоступна = ИСТИНА
	И ОтчетКомиссионера.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийОтчетКомиссионера.ОтчетКомиссионера)

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	РасходСоСчета.Ссылка,
	РасходСоСчета.Дата,
	РасходСоСчета.Организация,
	РасходСоСчета.Контрагент,
	НЕОПРЕДЕЛЕНО,
	РасходСоСчета.СуммаДокумента,
	ЛОЖЬ,
	ЗНАЧЕНИЕ(Перечисление.ДаНет.Нет)
ИЗ
	Документ.РасходСоСчета КАК РасходСоСчета
ГДЕ
	&amp;АвансыДоступны = ИСТИНА

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	РасходИзКассы.Ссылка,
