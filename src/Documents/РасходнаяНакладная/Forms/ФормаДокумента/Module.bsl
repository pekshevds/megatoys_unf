		Объект.Вес = 0;
		Объект.Объем = 0;
		Возврат;
	КонецЕсли;
	
	Объект.Вес = Объект.Запасы.Итог("Вес");
	Объект.Объем = Объект.Запасы.Итог("Объем");
	
