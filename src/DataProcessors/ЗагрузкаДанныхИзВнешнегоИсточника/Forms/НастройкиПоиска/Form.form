    <id>56</id>
    <items xsi:type="form:Decoration">
      <name>Подсказка</name>
      <id>58</id>
      <title>
        <key>ru</key>
        <value>Как это работает</value>
      </title>
      <visible>true</visible>
      <enabled>true</enabled>
      <userVisible>
