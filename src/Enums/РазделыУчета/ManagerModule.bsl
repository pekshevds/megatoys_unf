	Результат[Метаданные.РегистрыНакопления.ЗапасыНаСкладах] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ЗапасыПереданные] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ЗапасыПереданныеВРазрезеГТД] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ЗапасыПринятыеВРазрезеГТД] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ПартииТоваров] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ПотребностьВЗапасах] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ПрослеживаемыеТовары] = Запасы;
	Результат[Метаданные.РегистрыНакопления.СерииНоменклатуры] = Запасы;
	Результат[Метаданные.РегистрыНакопления.ДенежныеСредства] = ДенежныеСредства;
	Результат[Метаданные.РегистрыНакопления.ДоходыИРасходыКассовыйМетод] = РасчетыСПоставщикамиИПокупателями;
	Результат[Метаданные.РегистрыНакопления.ДоходыИРасходыНераспределенные] = РасчетыСПоставщикамиИПокупателями;
	Результат[Метаданные.РегистрыНакопления.ДоходыИРасходыОтложенные] = РасчетыСПоставщикамиИПокупателями;
	Результат[Метаданные.РегистрыНакопления.РасчетыСПоставщиками] = РасчетыСПоставщикамиИПокупателями;
	Результат[Метаданные.РегистрыНакопления.РасчетыСПокупателями] = РасчетыСПоставщикамиИПокупателями;
	Результат[Метаданные.РегистрыНакопления.РасчетыСПодотчетниками] = РасчетыСПодотчетниками;
	Результат[Метаданные.РегистрыНакопления.РасчетыСПрочимиКонтрагентами] = РасчетыСПрочимиКонтрагентами;
	Результат[Метаданные.РегистрыНакопления.РасчетыСПерсоналом] = РасчетыСПерсоналом;
	Результат[Метаданные.РегистрыНакопления.ВнеоборотныеАктивы] = Имущество;
	Результат[Метаданные.РегистрыНакопления.ВыработкаВнеоборотныхАктивов] = Имущество;
	Результат[Метаданные.РегистрыНакопления.РасчетыПоНалогам] = РасчетыПоНалогам;
	Результат[Метаданные.РегистрыНакопления.РасчетыПоЭквайрингу] = Эквайринг;
	Результат[Метаданные.РегистрыНакопления.ЗакупкиДляКУДиР] = ЗакупкиТоваровДляУСН;
	Результат[Метаданные.РегистрыНакопления.ПартииТоваровДляКУДиР] = ЗакупкиТоваровДляУСН;
	Результат[Метаданные.РегистрыНакопления.РасчетыСФондамиПоСтраховымВзносамУНФ] = РасчетыСФондамиПоСтраховымВзносам;
	Результат[Метаданные.РегистрыНакопления.РезервыТоваровОрганизаций] = РезервыТоваровОрганизаций;
	Результат[Метаданные.РегистрыНакопления.РасчетыПоКредитамИЗаймам] = РасчетыПоКредитамИЗаймам;
	Результат[Метаданные.РегистрыНакопления.БонусныеБаллы] = СкидкиБонусы;
	Результат[Метаданные.РегистрыНакопления.ПодарочныеСертификаты] = СкидкиБонусы;
	Результат[Метаданные.РегистрыНакопления.ПродажиПоДисконтнымКартам] = СкидкиБонусы;
	
	Возврат Результат;
	
